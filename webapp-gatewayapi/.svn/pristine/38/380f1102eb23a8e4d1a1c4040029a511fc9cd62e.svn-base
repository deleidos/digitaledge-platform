	private static JSONObject registerGateway() throws Exception{
		System.out.println("REGISTER GATEWAY");
		HttpURLConnection c = getConnection(POST,connectionStr+"account/register_gateway");
		BufferedWriter wtr = new BufferedWriter(new OutputStreamWriter(c.getOutputStream()));
		StringBuilder params = new StringBuilder();
		params.append("accountId=").append("90");
		params.append("&region=").append("us-east-1");
		params.append("&gatewayIP=").append("10.80.23.18");
		params.append("&gatewayInstanceId=").append("i-3c04ff46");
		wtr.write(params.toString());
		wtr.flush();
		wtr.close();
		c.getOutputStream().flush();
		c.getOutputStream().close();
		BufferedReader rdr = new BufferedReader(new InputStreamReader(c.getInputStream()));
		String line = rdr.readLine();
		JSONObject ret=null;
		while(line!=null){
			ret = JSONObject.fromObject(line);
			System.out.println(ret.toString(4));
			line = rdr.readLine();
		}
		System.out.println("REGISTER GATEWAY");
		return ret;
	}
	
	
