	import org.robotlegs.base.ContextEvent;
	import org.robotlegs.mvcs.Context;
	
	public class SystemBuilderRootContext extends Context
	{
		public function SystemBuilderRootContext(contextView:DisplayObjectContainer=null, autoStartup:Boolean=true)
		{
			super(contextView, autoStartup);
		}
		
		override public function startup():void
		{
			// Models
			var systemBuilderPropsModel:SystemBuilderPropertiesModel = new SystemBuilderPropertiesModel();
			injector.mapValue(IAppPropertiesModel, systemBuilderPropsModel);
			injector.mapValue(SystemBuilderPropertiesModel, systemBuilderPropsModel);
			
			injector.mapSingletonOf(IUser, User);
			injector.mapSingleton(DataModelModel);
			injector.mapSingleton(SystemPropertiesModel);
			injector.mapSingleton(TransportModel);
			injector.mapSingleton(SplitterModel);
			injector.mapSingleton(DatasinkModel);
			injector.mapSingleton(WebappModel);
			injector.mapSingleton(ProcessGroupModel);
			injector.mapSingleton(DataModelDirectory);
			injector.mapSingleton(VpcNetworkModel);
			
			// Dao Table singletons
			injector.mapSingleton(AvailabilityZones);
			injector.mapSingleton(DatasinkConfig);
			injector.mapSingleton(DatasinkAnnotations);
			injector.mapSingleton(TransportAnnotations);
			injector.mapSingleton(SplitterAnnotations);
			injector.mapSingleton(IaasAccounts);
			injector.mapSingleton(InstanceTypes);
			injector.mapSingleton(ProcessGroupConfig);
			injector.mapSingleton(SoftwareReleases);
			injector.mapSingleton(SystemSizing);
			injector.mapSingleton(TenantAccountAccess);
			injector.mapSingleton(WebappConfig);
			injector.mapSingleton(CurrentDefaults);
			injector.mapSingleton(ConfigListModel);
			injector.mapSingleton(SystemListModel);
			
			// Services
			//injector.mapSingletonOf(IPropertiesService, SimulatedPropertiesService);
			injector.mapClass(IPropertiesService, PropertiesService);
			//injector.mapSingletonOf(IUserServices, MockUserServices);
			injector.mapSingletonOf(IUserServices, UserServices);
			
			injector.mapClass(IThemeService, ThemeService);
			
			// injector.mapSingletonOf(IIngestApiServices, SimulatedIngestApiServices);
			injector.mapSingletonOf(IIngestApiServices, IngestApiServices );
			
			//  injector.mapSingletonOf(IDataModelServices, SimulatedDataModelServices);
			injector.mapSingletonOf(IDataModelServices, DataModelService);
			
			// injector.mapSingletonOf(ITmsDatabaseServices, SimulatedTmsDatabaseServices);
			injector.mapSingletonOf(ITmsDatabaseServices, TmsDatabaseService);
			
			// injector.mapSingletonOf(ISystemBuilderServices, SimulatedSystemBuilderServices);
			injector.mapSingletonOf(ISystemBuilderServices, SystemBuilderService);
			
			// injector.mapSingletonOf(IRepositoryServices, SimulatedRepositoryServices);
			injector.mapSingletonOf(IRepositoryServices, RepositoryServices);
			
			// injector.mapSingletonOf(IAccountService, SimulatedAccountService);
			injector.mapSingletonOf(IAccountService, AccountService);
			
			injector.mapSingletonOf(ISystemsService, SystemsService);
			
			// Other
			injector.mapSingleton(LoadStateManager);
			
			// Commands
			commandMap.mapEvent(PropertiesEvent.FETCH_REMOTE_PROPS_REQUEST, LoadRemotePropsIntoModelCommand);
			commandMap.mapEvent(ClientSessionTimerEvent.RESTART_SESSION_TIMER, RestartClientSessionTimerCommand);
			
			commandMap.mapEvent(UserOpRequestEvent.LOAD_USER_INFO, UserOpRequestCommand);
			commandMap.mapEvent(ThemeOpRequestEvent.LOAD_THEME, ThemeOpRequestCommand);
			
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_AVAILABILITY_ZONES, LoadAvailabilityZonesCommand, DataLoadOperationEvent);
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_DATASINK_ANNOTATIONS, LoadDatasinkAnnotationsCommand, DataLoadOperationEvent);
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_TRANSPORT_ANNOTATIONS, LoadTransportAnnotationsCommand, DataLoadOperationEvent);
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_SPLITTER_ANNOTATIONS, LoadSplitterAnnotationsCommand, DataLoadOperationEvent);
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_DATASINK_CONFIG, LoadDatasinkConfigCommand, DataLoadOperationEvent);
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_IAAS_ACCOUNTS, LoadIaasAccountsCommand, DataLoadOperationEvent);
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_PROCESS_GROUP_CONFIG, LoadProcessGroupConfigCommand, DataLoadOperationEvent);
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_SOFTWARE_RELEASES, LoadSoftwareReleasesCommand, DataLoadOperationEvent);
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_INSTANCE_TYPES, LoadInstanceTypesCommand, DataLoadOperationEvent);
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_SYSTEM_SIZING, LoadSystemSizingCommand, DataLoadOperationEvent);
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_TENANT_ACCOUNT_ACCESS, LoadTenantAccountAccessCommand, DataLoadOperationEvent);
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_WEBAPP_CONFIG, LoadWebappConfigCommand, DataLoadOperationEvent);
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_CURRENT_DEFAULTS, LoadCurrentDefaultsCommand, DataLoadOperationEvent);
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_ACCOUNT_AZONES, LoadRegionAzoneMappingCommand, DataLoadOperationEvent);
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_VPC_NETWORK, LoadVpcNetworkCommand, DataLoadOperationEvent);
			
			commandMap.mapEvent(EditEvent.ADD_DATAMODEL, AddDataModelCommand, EditEvent);
			commandMap.mapEvent(EditEvent.ADD_DATASINK, AddDatasinkCommand, EditEvent);
			commandMap.mapEvent(EditEvent.ADD_TRANSPORT, AddTransportCommand, EditEvent);
			commandMap.mapEvent(EditEvent.ADD_WEBAPP, AddWebappCommand, EditEvent);
			commandMap.mapEvent(EditEvent.REMOVE_DATAMODEL, RemoveDataModelCommand, EditEvent);
			commandMap.mapEvent(EditEvent.REMOVE_DATASINK, RemoveDatasinkCommand, EditEvent);
			commandMap.mapEvent(EditEvent.REMOVE_TRANSPORT, RemoveTransportCommand, EditEvent);
			commandMap.mapEvent(EditEvent.REMOVE_WEBAPP, RemoveWebappCommand, EditEvent);
			
			commandMap.mapEvent(ModelServiceEvent.LOAD_MODEL_DIR, LoadDataModelDirectoryCommand, ModelServiceEvent);
			commandMap.mapEvent(ModelServiceEvent.LOAD_DATA_MODEL, LoadDataModelCommand, ModelServiceEvent);
			commandMap.mapEvent(PopupEvent.SELECT_MODEL, OpenModelDialogCommand, PopupEvent);
			commandMap.mapEvent(SystemConfigServiceEvent.NEW_SYSTEM_CONFIG, NewSystemConfigCommand, SystemConfigServiceEvent);
			commandMap.mapEvent(ModelServiceEvent.LOAD_TENANT_SYSTEMS, LoadTenantSystems, ModelServiceEvent);
			commandMap.mapEvent(SystemConfigServiceEvent.SAVE_SYSTEM_CONFIG, BuildSystemCommand, SystemConfigServiceEvent);
			commandMap.mapEvent(SystemConfigServiceEvent.DOMAIN_DUP_CHECK, DomainDupCheckCommand, SystemConfigServiceEvent);
			commandMap.mapEvent(SystemConfigServiceEvent.LOAD_SYSTEM_CONFIG, LoadSystemConfigCommand, SystemConfigServiceEvent);
			commandMap.mapEvent(SystemConfigServiceEvent.LOAD_CONFIG_VERSIONS, LoadConfigVersionsCommand, SystemConfigServiceEvent);
			commandMap.mapEvent(SystemConfigServiceEvent.ALLOCATE_PERSISTENT_IP, AllocatePersistentIpCommand, SystemConfigServiceEvent);
			commandMap.mapEvent(SystemConfigServiceEvent.CREATE_PROCESS_GROUP, CreateProcessGroupCommand, SystemConfigServiceEvent);
			commandMap.mapEvent(SystemConfigServiceEvent.CREATE_DATASINK, CreateDatasinkCommand, SystemConfigServiceEvent);
			commandMap.mapEvent(SystemConfigServiceEvent.DELETE_DATASINK, DeleteDatasinkCommand, SystemConfigServiceEvent);
			commandMap.mapEvent(SystemConfigServiceEvent.DELETE_PROCESS_GROUP, DeleteProcessGroupCommand, SystemConfigServiceEvent);
			commandMap.mapEvent(SystemConfigServiceEvent.RELOAD_PROCESS_CONFIG, ReloadProcessGroupConfig, SystemConfigServiceEvent);
			
			// Mediators
			mediatorMap.mapView(HomeView, HomeViewMediator);
			mediatorMap.mapView(ParameterView, ParameterViewMediator);
			mediatorMap.mapView(AppLaunchStatusDisplay, AppLaunchStatusDisplayMediator);
			mediatorMap.mapView(UserDisplay, UserDisplayMediator, IUserDisplay);
			mediatorMap.mapView(AboutDialog, AboutDialogMediator, null, false, false);
			mediatorMap.mapView(SystemBuilder, SystemBuilderMediator);
			mediatorMap.createMediator(contextView);
			mediatorMap.createMediator((contextView as SystemBuilder).launchStatusDisplay);
			
			// Popups with Mediators
			mediatorMap.mapView(OpenDataModelPopup, OpenDataModelPopupMediator, null, false, false);
			mediatorMap.mapView(OpenOrNewPopup,		OpenOrNewPopupMediator,     null, false, false);
			
			// Startup event
			commandMap.mapEvent(ContextEvent.STARTUP_COMPLETE, StartupCommand, ContextEvent, true);
			
			super.startup();
		}
	}
}
