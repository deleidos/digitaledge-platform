package com.deleidos.rtws.tc.views
{
	import com.deleidos.rtws.commons.model.auth.IUser;
	import com.deleidos.rtws.commons.model.auth.Role;
	import com.deleidos.rtws.commons.model.auth.User;
	import com.deleidos.rtws.commons.model.status.StatusMessage;
	import com.deleidos.rtws.tc.events.AccountEvent;
	import com.deleidos.rtws.tc.events.AccountsEvent;
	import com.deleidos.rtws.tc.events.ApplicationEvent;
	import com.deleidos.rtws.tc.events.NavigationEvent;
	import com.deleidos.rtws.tc.events.RepositoryEvent;
	import com.deleidos.rtws.tc.events.TenantAccountCheckerEvent;
	import com.deleidos.rtws.tc.events.UserEvent;
	import com.deleidos.rtws.tc.models.Account;
	import com.deleidos.rtws.tc.models.AccountModel;
	import com.deleidos.rtws.tc.models.UserModel;
	import com.deleidos.rtws.tc.utils.IAASServiceUtilities;
	import com.deleidos.rtws.tc.utils.NotificationEventBuilder;
	import com.deleidos.rtws.tc.utils.StatusMessageFactory;
	import com.deleidos.rtws.tc.utils.ViewUtilities;
	import com.deleidos.rtws.tc.views.popups.AccountView;
	import com.deleidos.rtws.tc.views.popups.ConfirmationView;
	
	import flash.events.Event;
	import flash.events.MouseEvent;
	
	import mx.managers.PopUpManager;
	
	import org.robotlegs.mvcs.Mediator;
	
	import spark.collections.Sort;
	import spark.collections.SortField;
	import spark.events.GridSelectionEvent;

	public class AccountsViewMediator extends Mediator
	{
		[Inject] public var view:AccountsView;
		[Inject] public var iUser:IUser;
		[Inject] public var accountModel:AccountModel;
		[Inject] public var userModel:UserModel;
		
		public override function onRegister():void 
		{
			view.accountsControlBar.setStyle("dropShadowEnabled", "false");
			
			eventMap.mapListener(eventDispatcher, AccountsEvent.REFRESH, handleRefreshEvent);
			eventMap.mapListener(eventDispatcher, AccountsEvent.REFRESH_COMPLETE, handleRefreshCompleteEvent);
			eventMap.mapListener(eventDispatcher, AccountsEvent.RETRIEVE_TENANT_SUCCESS, handleRetrieveTenantSuccessEvent);
			
			eventMap.mapListener(eventDispatcher, AccountEvent.DISPLAY_TENANT, handleDisplayTenantEvent);
			if (iUser.isUserInRole(new Role("ROLE_ADMIN"))) {
				eventMap.mapListener(eventDispatcher, TenantAccountCheckerEvent.VERIFY_ACCOUNTS_STATUS, handleVerifyAccountsStatusEvent);
			}
			
			view.addAccountButton.addEventListener(MouseEvent.CLICK, handleAddAccountMouseClick);
			view.editAccountButton.addEventListener(MouseEvent.CLICK, handleEditAccountMouseClick);
			view.deleteAccountButton.addEventListener(MouseEvent.CLICK, handleDeleteAccountMouseClick);
			view.tenantDataGrid.addEventListener(GridSelectionEvent.SELECTION_CHANGE, handleDataGridSelectionChangeEvent);
			
			view.refreshButton.addEventListener(MouseEvent.CLICK, handleRefreshEvent);
			
			view.dataProvider = accountModel.accounts;
			if (view.dataProvider.length > 1) {
				var sortField:SortField = new SortField();
				sortField.name = "tenantId";
				sortField.numeric = false;
				var sort:Sort = new Sort();
				sort.fields = [sortField];
				
				view.dataProvider.sort = sort;
				view.dataProvider.refresh();
			}
			
			setAddButtonEnabledState();

			view.currentState = view.blankState.name;
		}
		
		protected function handleRefreshEvent(event:Event):void
		{
			var eventQueue:Array = new Array();
			eventQueue.push(new AccountsEvent(AccountsEvent.RETRIEVE_NAMES));
			
			var successEvents:Array = new Array();
			successEvents.push(AccountsEvent.RETRIEVE_NAMES_SUCCESS);
			
			var errorEvents:Array = new Array();
			errorEvents.push(AccountsEvent.RETRIEVE_NAMES_ERROR);
			
			var onCompleteEvent:AccountsEvent = new AccountsEvent(AccountsEvent.REFRESH_COMPLETE);
			var onErrorEvent:ApplicationEvent = NotificationEventBuilder.buildErrorMessage("Refresh Accounts", 
				"Failed to retrieve tenant names.", event);
			
			ViewUtilities.dispatchProcessingView(mediatorMap, contextView, eventQueue, successEvents, errorEvents, true, onCompleteEvent);
		}
		
		protected function handleAddAccountMouseClick(event:MouseEvent):void
		{
			showAccountViewPopup();
		}
		
		protected function handleEditAccountMouseClick(event:MouseEvent):void
		{
			var popup:AccountView = new AccountView();
				
			popup.title = "Update Account";
			popup.account = Account(view.tenantDataGrid.selectedItem);
			
			PopUpManager.addPopUp(popup, contextView, true);
			PopUpManager.centerPopUp(popup);
				
			mediatorMap.createMediator(popup); 
		}
		
		protected function handleDeleteAccountMouseClick(event:MouseEvent):void
		{
			var account:Account = Account(view.tenantDataGrid.selectedItem);
				
			var eventQueue:Array = new Array();
			eventQueue.push(new AccountEvent(AccountEvent.DELETE_ACCOUNT, {"tenantId" : account.tenantId}));
				
			var successEvents:Array = new Array();
			successEvents.push(AccountEvent.DELETE_ACCOUNT_SUCCESS);
				
			var errorEvents:Array = new Array();
			errorEvents.push(AccountEvent.DELETE_ACCOUNT_ERROR);
				
			var onCompleteEvent:ApplicationEvent = NotificationEventBuilder.buildSuccessMessage("Delete Account", 
				"Successfully deleted account for tenant '" + account.tenantId + "'.", new AccountsEvent(AccountsEvent.REFRESH));
			var onErrorEvent:ApplicationEvent = NotificationEventBuilder.buildErrorMessage("Delete Account", 
				"Failed to delete account for tenant '" + account.tenantId + "'.");
				
			var popup:ConfirmationView = new ConfirmationView();
				
			popup.popup = ViewUtilities.buildProcessingView(eventQueue, successEvents, errorEvents, false, onCompleteEvent, onErrorEvent);
			popup.message = StatusMessageFactory.createWarnMessage("Are you sure you want to delete the selected account?");
				
			PopUpManager.addPopUp(popup, contextView, true);
			PopUpManager.centerPopUp(popup);
				
			mediatorMap.createMediator(popup);				
		}
		
		protected function handleDataGridSelectionChangeEvent(event:GridSelectionEvent):void
		{
			if (view.tenantDataGrid.selectedItem != null) {
				view.currentState = view.blankState.name;
				
				var account:Account = Account(view.tenantDataGrid.selectedItem);
				
				var eventQueue:Array = new Array();
				eventQueue.push(new AccountsEvent(AccountsEvent.RETRIEVE_TENANT, {"tenantId" : account.tenantId}));
				
				var successEvents:Array = new Array();
				successEvents.push(AccountsEvent.RETRIEVE_TENANT_SUCCESS);
				
				var errorEvents:Array = new Array();
				errorEvents.push(AccountsEvent.RETRIEVE_TENANT_ERROR);
				
				var onErrorEvent:ApplicationEvent = NotificationEventBuilder.buildErrorMessage("Tenant Account", 
					"Failed to retrieve tenant account information.");
				
				ViewUtilities.dispatchProcessingView(mediatorMap, contextView, eventQueue, successEvents, errorEvents, false, null, onErrorEvent);
			}
		}
		
		protected function handleCreateUserLoginMouseClick(event:MouseEvent):void
		{
			dispatch(new NavigationEvent(NavigationEvent.CHANGE_VIEW, {"view" : NavigationEvent.USERS_VIEW, 
				"event" : new UserEvent(UserEvent.INVOKE_ADD_USER_POPUP, {"tenantId" : view.tenantIdLabel.text})}));
		}
		
		protected function handleCreateRepositoryMouseClick(event:MouseEvent):void
		{
			var account:Account = Account(view.tenantDataGrid.selectedItem);
				
			var eventQueue:Array = new Array();
			eventQueue.push(new RepositoryEvent(RepositoryEvent.ADD_TENANT, {tenantId:account.tenantId, accountId:account.accountId}));
				
			var successEvents:Array = new Array();
			successEvents.push(RepositoryEvent.ADD_TENANT_SUCCESS);
				
			var errorEvents:Array = new Array();
			errorEvents.push(RepositoryEvent.ADD_TENANT_ERROR);

			var onCompleteEvent:ApplicationEvent = NotificationEventBuilder.buildSuccessMessage("Create Repository", 
				"Successfully created repository for tenant '" + account.tenantId + "'.");
			var onErrorEvent:ApplicationEvent = NotificationEventBuilder.buildErrorMessage("Create Repository", 
				"Failed to create repository for tenant '" + account.tenantId + "'.");
				
			var popup:ConfirmationView = new ConfirmationView();
			
			popup.popup = ViewUtilities.buildProcessingView(eventQueue, successEvents, errorEvents, false, onCompleteEvent, onErrorEvent);
			popup.message = StatusMessageFactory.createWarnMessage("Are you sure you want to create a repository for the selected account?");
				
			PopUpManager.addPopUp(popup, contextView, true);
			PopUpManager.centerPopUp(popup);
				
			mediatorMap.createMediator(popup);
		}
		
		protected function handleCreateDnsEntryMouseClick(event:MouseEvent):void
		{
			var account:Account = Account(view.tenantDataGrid.selectedItem);
			
			var eventQueue:Array = new Array();
			eventQueue.push(new AccountEvent(AccountEvent.ADD_DNS_ENTRY, {"tenantId" : account.tenantId }));
			
			var successEvents:Array = new Array();
			successEvents.push(AccountEvent.ADD_DNS_ENTRY_SUCCESS);
			
			var errorEvents:Array = new Array();
			errorEvents.push(AccountEvent.ADD_DNS_ENTRY_ERROR);
			
			var onCompleteEvent:AccountsEvent = new AccountsEvent(AccountsEvent.REFRESH);
			var onErrorEvent:ApplicationEvent = NotificationEventBuilder.buildErrorMessage("Create DNS Entry", 
				"Failed to add dns entry.");
			
			ViewUtilities.dispatchProcessingView(mediatorMap, contextView, eventQueue, successEvents, errorEvents, true, onCompleteEvent, onErrorEvent);
		}
		
		protected function handleRefreshCompleteEvent(event:AccountsEvent):void
		{
			view.editAccountButton.enabled = false;
			view.deleteAccountButton.enabled = false;
			
			view.currentState = view.blankState.name;
			
			view.dataProvider.refresh();
			view.tenantDataGrid.selectedIndex = -1;
			
			setAddButtonEnabledState();
			
			if (iUser.isUserInRole(new Role("ROLE_ADMIN"))) { 
				dispatch(new TenantAccountCheckerEvent(TenantAccountCheckerEvent.VERIFY_ACCOUNTS));
			}
		}
		
		protected function handleRetrieveTenantSuccessEvent(event:AccountsEvent):void
		{
			if (event.parameters != null && event.parameters.context != null && event.parameters.context.tenantId != null) {
				var account:Account = accountModel.findByTenantId(event.parameters.context.tenantId);
				populateAccountDetailView(account);
			}
		}
		
		protected function handleDisplayTenantEvent(event:AccountEvent):void
		{
			if (event.parameters != null && event.parameters.tenantId != null) 
			{
				for (var i:int = 0; i < view.tenantDataGrid.dataProvider.length; i++)
				{
					var account:Account = Account(view.tenantDataGrid.dataProvider.getItemAt(i));
					if (account.tenantId == event.parameters.tenantId)
					{
						view.tenantDataGrid.selectedItem = account;
						populateAccountDetailView(account);
						return;
					}
				}
			} 
				
			showAccountViewPopup(event);
		}
		
		protected function handleVerifyAccountsStatusEvent(event:TenantAccountCheckerEvent):void
		{
			if (event.parameters != null)
			{
				view.statusMessage = event.parameters.statusMessage;
			}
		}
		
		private function showAccountViewPopup(event:AccountEvent = null):void
		{
			var popup:AccountView = new AccountView();
			
			popup.title = "Create New Account";
			if (event != null && event.parameters != null && event.parameters.tenantId) {
				popup.tenantId = event.parameters.tenantId;
			}
			
			PopUpManager.addPopUp(popup, contextView, true);
			PopUpManager.centerPopUp(popup);
			
			mediatorMap.createMediator(popup);
		}
		
		private function populateAccountDetailView(account:Account):void
		{
			setViewState(account);
			
			view.tenantIdLabel.text = account.tenantId;
			view.primaryLabel.text = (account.isPrimary) ? "yes" : "no";
			view.cloudIdLabel.text = account.cloudId;
			view.descriptionLabel.text = account.description;
			view.iaasServiceNumberLabel.text = account.iaasServiceNumber;
			view.iaasServiceLabel.text = account.iaasServiceName;
			view.keyPairLabel.text = account.keyPair;
			view.domainSuffixLabel.text = account.domainNameSuffix;
			
			if (IAASServiceUtilities.isAWS(account.iaasServiceName)) {
				view.cloudIdFormItem.label = "Canonical Id:";
			} else if (IAASServiceUtilities.isEUC(account.iaasServiceName)) {
				view.cloudIdFormItem.label = "Account Id:";
			}
			
			view.editAccountButton.enabled = true;
			view.deleteAccountButton.enabled = true;
			
			if (userModel.findUser(account.tenantId) == null) {
				view.userImage.styleName = "cross";
				view.userLoginButton.mouseEnabled = true;
				view.userLoginButton.addEventListener(MouseEvent.CLICK, handleCreateUserLoginMouseClick);
			} else {
				view.userImage.styleName = "check";
				view.userLoginButton.removeEventListener(MouseEvent.CLICK, handleCreateUserLoginMouseClick);
			}
			
			if (account.gatewayInstanceId == null) {
				view.gatewayImage.styleName = "cross";
			} else {
				view.gatewayImage.styleName = "check";
				view.gatewayImage.toolTip = account.gatewayInstanceId;
			}
			
			if (account.isPrimary && ! (account.tenantId == "admin")) {
				if (account.isRepositorySpaceCreated == false) {
					view.repositoryImage.styleName = "cross";
					view.repositorySpaceButton.mouseEnabled = true;
					view.repositorySpaceButton.addEventListener(MouseEvent.CLICK, handleCreateRepositoryMouseClick);
				} else {
					view.repositoryImage.styleName = "check";
					view.repositorySpaceButton.removeEventListener(MouseEvent.CLICK, handleCreateRepositoryMouseClick);
				}
			}
			
			if (IAASServiceUtilities.isEUC(account.iaasServiceName) && account.isPrimary && ! (account.tenantId == "admin")) {
				if (account.isDnsForwarderSet == false) {
					view.dnsForwarderImage.styleName = "cross";
					view.dnsForwarderButton.mouseEnabled = true;
					view.dnsForwarderButton.addEventListener(MouseEvent.CLICK, handleCreateDnsEntryMouseClick);
				} else {
					view.dnsForwarderImage.styleName = "check";
					view.dnsForwarderButton.removeEventListener(MouseEvent.CLICK, handleCreateDnsEntryMouseClick);
				}
			
				if (account.isDnsZoneSet == false) {
					view.dnsZoneImage.styleName = "cross";
					view.dnsZoneButton.mouseEnabled = true;
					view.dnsZoneButton.addEventListener(MouseEvent.CLICK, handleCreateDnsEntryMouseClick);
				} else {
					view.dnsZoneImage.styleName = "check";
					view.dnsZoneButton.removeEventListener(MouseEvent.CLICK, handleCreateDnsEntryMouseClick);
				}
			}
		}
		
		private function setViewState(account:Account):void
		{
			if (account.tenantId == "admin") {
				view.currentState = view.selectedAdminState.name;
			} else if (IAASServiceUtilities.isEUC(account.iaasServiceName) && account.isPrimary) {
				view.currentState = view.selectedEucPrimaryState.name;
			} else if (IAASServiceUtilities.isEUC(account.iaasServiceName) && ! account.isPrimary) {
				view.currentState = view.selectedEucSecondaryState.name;
			} else if (! IAASServiceUtilities.isEUC(account.iaasServiceName) && account.isPrimary) {
				view.currentState = view.selectedAwsPrimaryState.name;
			} else {
				view.currentState = view.selectedAwsSecondaryState.name;
			}
		}
		
		private function setAddButtonEnabledState():void
		{
			for (var i:int = 0; i < userModel.users.length; i++) {
				var user:User = User(userModel.users.getItemAt(i));	
				if (accountModel.findByTenantId(user.username) == null) {
					view.addAccountButton.enabled = true;
				}
			}
		}
	}
}