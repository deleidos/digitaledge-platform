package com.saic.rtws.commons.model.tmsdb;

/**
 * TMS DB TransportConfig DTO object.
 * 
 * @author LUCECU
 * 
 * @deprecated this database table no longer exists
 *
 */
public class TransportConfig {

	/**
	 * Fully qualified name.
	 */
	private String fqn;
	/**
	 * Description of transport.
	 */
	private String description;
	/**
	 * Transport configuration xml template.
	 */
	private String transportXmlTemplate;
	
	/**
	 * Default Constructor.
	 */
	public TransportConfig(){
		
	}
	
	/**
	 * Set the fully qualified name.
	 * 
	 * @param fqn String value
	 */
	public void setFqn(String fqn){
		this.fqn = fqn;
	}
	
	/**
	 * Get the fully qualified name.
	 * 
	 * @return String value
	 */
	public String getFqn(){
		return fqn;
	}
	
	/**
	 * Set the transport description;
	 * 
	 * @param description String value
	 */
	public void setDescription(String description){
		this.description = description;
	}
	
	/**
	 * Get the transport description;
	 * 
	 * @return String value
	 */
	public String getDescription(){
		return description;
	}
	
	/**
	 * Set the transport xml template.
	 * 
	 * @param transportXmlTemplate String value
	 */
	public void setTransportXmlTemplate(String transportXmlTemplate){
		this.transportXmlTemplate = transportXmlTemplate;
	}
	
	/**
	 * Get the transport xml template.
	 * 
	 * @return String value
	 */
	public String getTransportXmlTemplate(){
		return transportXmlTemplate;
	}
}
