			service = ServiceFactoryUtils.createService("com.deleidos.rtws.ext.processor.PostalLocationGeoSpatialEnrichment", 
					ingestExtUrl, ServiceType.PROCESSOR);
			assertTrue(service instanceof ProcessorService);
			
			ProcessorService postService = (ProcessorService) service;
			
			assertEquals("com.deleidos.rtws.ext.processor.PostalLocationGeoSpatialEnrichment", postService.getFqn());
			assertTrue(StringUtils.isNotBlank(postService.getDescription()));
