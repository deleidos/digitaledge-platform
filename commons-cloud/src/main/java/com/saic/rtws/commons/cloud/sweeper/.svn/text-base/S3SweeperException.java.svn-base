package com.saic.rtws.commons.cloud.sweeper;

/**
 * Exception for the s3sweeper application.
 */
public class S3SweeperException extends Exception {
	
	private static final long serialVersionUID = 7683971047569040306L;

	public S3SweeperException(String message) {
		super(message);
	}
	
	public S3SweeperException(String message, Throwable th) {
		super(message, th);
	}

}
