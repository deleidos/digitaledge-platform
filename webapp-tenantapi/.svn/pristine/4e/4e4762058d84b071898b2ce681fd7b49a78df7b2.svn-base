public class GatewayBuildTask extends BuildTask {

	private Logger logger = Logger.getLogger(getClass());
	
	// S3 paths
	private final static String TEMPLATE_SYSTEM_DOMAIN = "template-anchor.deleidos.com";
	private final static String CONFIGURATION_PATH = "configuration";
	private final static String MANIFEST_PATH = "manifest";
	private final static String RELEASE = "release";
	private final static String SCRIPTS = "scripts";
	private final static String SERVICES = "services";
	
	// Local working directory paths
	private final static String TEMP_DIR = "/tmp/";
	private final static String CONF_DIR = "/conf/";
	private final static String PROPS_DIR = "/props/";
	private final static String MANIFEST_DIR = "/manifest/";
	
	// GWPropertyComposer properties
	private final static String COMMON_PROPERTIES_TAR = "deleidos-rtws-common-properties-conf.tar.gz";
	// InterfaceConfigurationComposer properties
	private final static String COMMON_CONF_TAR = "deleidos-rtws-common-conf-conf.tar.gz";
	// GWManifestComposer properties
	private final static String ANCHOR_MANIFEST_FILE = "anchor.ini";

	// S3 values
	private StorageInterface storageInterface;
	private String swVersion;
	private String bucket;
	private String accountEmail;
	
	// Build Task properties
	private String tenantId;
	private String region;
	private String domain;
	private String azone;
	private SystemConfig config;
	private SystemContext systemContext;
	private RtwsProperties systemProperties;
	
	public GatewayBuildTask(String tenantId, String domain, String region, String azone) {
		this.tenantId = tenantId;
		this.domain = domain;
		this.region = region;
		this.azone = azone;
	}
	
	public static void main(String[] args) throws Exception {
		BuildServiceImpl buildServiceImpl = new BuildServiceImpl();
		String resp = buildServiceImpl.createGateway("dev-test2", "deleidos.com", "us-east-1", "us-east-1c");
		String oldStat = BuildManager.SINGLETON.getStatus(resp);
		System.out.println(oldStat);
		while(true){
			String stat = BuildManager.SINGLETON.getStatus(resp);
			if(!stat.equals(oldStat)){
				System.out.println(stat);
				oldStat = stat;
			}
			try {
				Thread.sleep(1);
			} catch (InterruptedException e) {
			}
		}
	}
	
	@Override
	public void run() {
		try {
			systemProperties = new RtwsProperties();
			config = buildSystemConfig();
			systemContext = buildSystemContext();
			storageInterface = buildStorageInterface();
			
			// start with copying all the template config file into the tenant's bucket (direct bucket to bucket copy)
			copyTemplateConfiguration();
			// pull the files down that the composers will modify
			pullTemplateConfiguration();
			
			List<DefinitionComposer> composers = getComposerList();
			//each composer has 6 operations - assume they all take the same quantity of time (probably wrong, but easy for now)
			double progressPerAction = 90.0/(composers.size()*6);//reserve 10% for output handling
			double totalProgress=0;
			
			for (int i=0; i < composers.size(); i++){
				//totalProgress = i*progressPerComposer;
				BuildManager.SINGLETON.setStatus(token, (int)totalProgress);
				DefinitionComposer composer = composers.get(i);
				composer.initialize();

				totalProgress+=progressPerAction;
				BuildManager.SINGLETON.setStatus(token, (int)totalProgress);
				composer.loadDefaults(null, config, systemContext, systemProperties);

				totalProgress+=progressPerAction;
				BuildManager.SINGLETON.setStatus(token, (int)totalProgress);
				ComposerStatus stat = composer.compose(config, systemContext);
				if(stat!=ComposerStatus.done){
					BuildManager.SINGLETON.setErrorStatus(token, "Return from composer was "+stat+".  Processing will stop.  Composer was of type "+composer.getClass()+".");
					return;
				}
				else
					logger.debug("successfully finished composer: "+composer.getClass()+ " for build of system: "+this.config.getSystemDomain());

				totalProgress+=progressPerAction;
				BuildManager.SINGLETON.setStatus(token, (int)totalProgress);
				composer.writeFile(null, null);

				totalProgress+=progressPerAction;
				BuildManager.SINGLETON.setStatus(token, (int)totalProgress);
				composer.dispose();

				totalProgress+=progressPerAction;
				BuildManager.SINGLETON.setStatus(token, (int)totalProgress);
				systemProperties = (RtwsProperties)composer.getProperties();
				totalProgress+=progressPerAction;
			}
			
			// push the modified config files up to the tenant's bucket, overriding the defaults 
			pushTemplateConfiguration();
			// finally set the new files as readable for the new tenant
			setTenantReadAcls();
			BuildManager.SINGLETON.setStatus(token, 100);
			logger.info("Finished building gateway system for domain: " + config.getSystemDomain() + " swVersion: " + swVersion);
			
			
		} catch (Exception e) {
			logBuildTaskException(token, e.getMessage(), e);
		}
	}
	
	private void copyTemplateConfiguration() throws StorageException {
		// Copy configuration, and manifest from template-anchor to the tenant's domainName bucket
		String[] paths = { CONFIGURATION_PATH, MANIFEST_PATH };
		for (String path: paths) {
			String fromPrefix =
					String.format("%s/%s/%s/", swVersion, path, TEMPLATE_SYSTEM_DOMAIN);
			String toPrefix = String.format("%s/%s/%s/", swVersion, path, domain);
			storageInterface.copyFiles(bucket, bucket, true, fromPrefix, toPrefix);
		}
	}
	
	private void setTenantReadAcls() throws StorageException {
		// set read acls on the new bucket files
		String[] tenantPaths = { CONFIGURATION_PATH, MANIFEST_PATH };
		for (String path: tenantPaths) {
			String toPrefix = String.format("%s/%s/%s/", swVersion, path, domain);
			storageInterface.changeFilesACL(bucket, toPrefix, accountEmail, Permission.PERMISSION_READ);
		}
		
		String[] rtwsPaths = { RELEASE, SCRIPTS, SERVICES };
		for (String path: rtwsPaths) {
			String toPrefix = String.format("%s/%s/", swVersion, path);
			storageInterface.changeFilesACL(bucket, toPrefix, accountEmail, Permission.PERMISSION_READ);
		}
		
		storageInterface.changeACLOnBucket(bucket, accountEmail, Permission.PERMISSION_READ, false);
		
	}
	
	private void pullTemplateConfiguration() throws StorageException, IOException {
		
		// GWPropertyComposer
		String fileKey = String.format("%s/%s/%s/%s", swVersion, CONFIGURATION_PATH, domain, COMMON_PROPERTIES_TAR);
		String outputDir = config.getOutputConfDir() + PROPS_DIR;
		
		File propTar = FileUtil.initializeFile(outputDir, COMMON_PROPERTIES_TAR);
		storageInterface.getFileWithRetry(bucket, fileKey, propTar, 3, 5000);
		
		systemProperties.put(GatewayPropertyComposer.OUTPUT_DIR_PROP, outputDir);
		systemProperties.put(GatewayPropertyComposer.TAR_NAME_PROP, COMMON_PROPERTIES_TAR);
		
		// GWManifestComposer
		fileKey = String.format("%s/%s/%s/%s", swVersion, MANIFEST_PATH, domain, ANCHOR_MANIFEST_FILE);
		outputDir = config.getOutputConfDir() + MANIFEST_DIR;
		
		File anchorManifest = FileUtil.initializeFile(outputDir, ANCHOR_MANIFEST_FILE);
		storageInterface.getFileWithRetry(bucket, fileKey, anchorManifest, 3, 5000);
		
		systemProperties.put(GatewayManifestComposer.OUTPUT_DIR_PROP, outputDir);
		systemProperties.put(GatewayManifestComposer.MANIFEST_NAME_PROP, ANCHOR_MANIFEST_FILE);
		
		// InterfaceConfigurationComposer
		fileKey = String.format("%s/%s/%s/%s", swVersion, CONFIGURATION_PATH, domain, COMMON_CONF_TAR);
		outputDir = config.getOutputConfDir() + CONF_DIR;
		
		File confTar = FileUtil.initializeFile(outputDir, COMMON_CONF_TAR);
		storageInterface.getFileWithRetry(bucket, fileKey, confTar, 3, 5000);
	}
	
	private void pushTemplateConfiguration() throws StorageException, IOException {
		String fileKey = String.format("%s/%s/%s/", swVersion, CONFIGURATION_PATH, domain);
		String outputDir = config.getOutputConfDir() + PROPS_DIR;
		storageInterface.putFiles(outputDir, bucket, fileKey);
		
		// the InterfaceConfigurationComposer doesn't re-tar the tar yet, do it here
		outputDir = config.getOutputConfDir() + CONF_DIR;
		File confTar = new File(outputDir, COMMON_CONF_TAR);
		File interfacesConfigFile = new File(outputDir, "interfaces-config.xml");
		FileUtil.replaceFileInTarGZip(confTar, interfacesConfigFile);
		interfacesConfigFile.delete();
		storageInterface.putFiles(outputDir, bucket, fileKey);
		
		fileKey = String.format("%s/%s/%s/", swVersion, MANIFEST_PATH, domain);
		outputDir = config.getOutputConfDir() + MANIFEST_DIR;
		storageInterface.putFiles(outputDir, bucket, fileKey);
	}
	
	private void logBuildTaskException(String token, String errorMsg, Exception e) {
		Writer result = new StringWriter();
		PrintWriter printWriter = new PrintWriter(result);
		e.printStackTrace(printWriter);
		logger.error(e.getMessage(),e);
		
		try {
			BuildManager.SINGLETON.setErrorStatus(token, errorMsg + " Exception info: " + result.toString());
		} catch (SystemBuildException e1) {
			logger.error("error setting error status for token "+token+".",e);
		}
		return;
	}
	
	private SystemConfig buildSystemConfig() throws RestClientException {
		config = new SystemConfig();
		config.setSystemDomain(domain);
		
		AccountEntry entry = CacheManager.getTenantAccountCache().get(tenantId);
		if (entry != null && entry.getIAASAccount() != null) {
			config.setServiceProvider(entry.getIAASAccount().getIaasServiceName());
		}
		
		config.setRegion(region);
		config.setAvailZone(azone);
		config.setSoftwareVersion(RestClientManager.getSoftwareReleasesClient().getCurrentSoftwareVersionId());
		config.setSystemSize("large");
		config.setAutoScale(false);
		config.setOutputConfDir(TEMP_DIR + domain);
		
		return config;
	}
	
	private SystemContext buildSystemContext() {
		DataSource ds = H2DataSourceFactory.getInstance().getDataSource();

		//setup dao objects
		AvailabilityZoneDao availabilityZoneDAO = new AvailabilityZoneDao();
		availabilityZoneDAO.setDataSource(ds);
		
		IAASConfigurationDao iaasConfigDAO = new IAASConfigurationDao();
		iaasConfigDAO.setDataSource(ds);

		ProcessGroupConfigDao processGroupDAO = new ProcessGroupConfigDao();
		processGroupDAO.setDataSource(ds);

		SystemSizingDao sizeDAO = new SystemSizingDao();
		sizeDAO.setDataSource(ds);

		MachineImageDao imageDAO = new MachineImageDao();
		imageDAO.setDataSource(ds);

		InstanceTypeDao instanceDAO = new InstanceTypeDao();
		instanceDAO.setDataSource(ds);

		DataSinkConfigDao dataSinkDAO = new DataSinkConfigDao();
		dataSinkDAO.setDataSource(ds);


		SystemContext systemContext = new SystemContext();
		systemContext.setAvailabilityZone(availabilityZoneDAO.getAvailabilityZoneByZone(config.getAvailZone()));
		systemContext.setIaasConfiguration(iaasConfigDAO.getIAASConfigurationByName(config.getServiceProvider()));
		systemContext.setProcessGroupConfigs(new ArrayList<ProcessGroupConfig>(processGroupDAO.getProcessGroupConfigs()));
		systemContext.setSystemSize(sizeDAO.getSystemBySize(config.getSystemSize()));
		systemContext.setMachineImages(new ArrayList<MachineImage>(imageDAO.getMachineImages()));
		systemContext.setInstanceTypes(new ArrayList<InstanceType>(instanceDAO.getInstanceTypes()));
		systemContext.setDataSinkConfigs((ArrayList<DataSinkConfig>) new ArrayList<DataSinkConfig>(dataSinkDAO.getDataSinkConfigs()));
		return systemContext;
	}

	private StorageInterface buildStorageInterface() throws RestClientException, SystemServiceException {
		
		swVersion = RestClientManager.getSoftwareReleasesClient().getCurrentSoftwareVersionId();
		bucket = RestClientManager.getAvailabilityZonesClient().getBucketForAvailabilityZone(azone);
		
		int maxRetries = 6;
		int retryCount = 0;
		AccountEntry entry = null;
		// adding a few retries since cache may not be loaded at webapp start
		while (entry == null && (retryCount < maxRetries)) {
			entry = CacheManager.getTenantAccountCache().get(tenantId);
			try {
				Thread.sleep(10000);
			} catch (InterruptedException e) {
				//e.printStackTrace();
			}
			retryCount++;
		}
		
		if (entry == null || entry.getIAASAccount() == null) {
			throw new SystemServiceException("Required tenant '" + tenantId + "' data [iaas account] is not available.");
		}
		accountEmail = entry.getIAASAccount().getAccountEmail();
		
		UserDataProperties udProps = UserDataProperties.getInstance();
		String tmsAccessKey = udProps.getString(UserDataProperties.RTWS_ACCESS_KEY);
		String tmsSecretKey = udProps.getString(UserDataProperties.RTWS_SECRET_KEY);
		
		StorageServiceClient ssDao = StorageServiceClient.create(tmsAccessKey, tmsSecretKey);
		StorageInterface storageInterface = ssDao.getStorageInterface();
		return storageInterface;
	}

	private List<DefinitionComposer> getComposerList() {
		List<DefinitionComposer> composers = new ArrayList<DefinitionComposer>();
		DefinitionComposer  t = new GatewayPropertyComposer();
		composers.add(t);
		
		t = new GatewayManifestComposer();
		composers.add(t);

		t = new InterfaceConfigurationComposer();
		composers.add(t);
		
		//------------ADD NEW COMPOSERS HERE-----------



		//------------ADD NEW COMPOSERS ABOVE THIS LINE------------
		return composers;
	}
	
}
