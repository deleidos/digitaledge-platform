<?xml version="1.0" encoding="utf-8"?>
<s:HGroup 
	xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark" 
	xmlns:mx="library://ns.adobe.com/flex/mx"
	xmlns:rtws="com.deleidos.rtws.commons.view.component.*"
	verticalAlign="middle">

	<rtws:StyleableIconButton id="clearButton" styleName="mediumIcon clear" toolTip="Clear" alpha="0.5" mouseOut="{clearButton.alpha = 0.5}" mouseOver="{clearButton.alpha = 1}"/>
	<rtws:StyleableIconButton id="settingsButton" styleName="mediumIcon settings" toolTip="Settings" alpha="0.5" mouseOut="{settingsButton.alpha = 0.5}" mouseOver="{settingsButton.alpha = 1}"/>
	<rtws:StyleableIconButton id="infoButton" styleName="mediumIcon information" toolTip="Information" alpha="0.5" mouseOut="{infoButton.alpha = 0.5}" mouseOver="{infoButton.alpha = 1}"/>
</s:HGroup>

