<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">

<!--
	README
	
	This feature is not used in the current demo version of AlertViz.  If used, this file
	should be dynamically generated to allow for proper replacement of the dynamic variables.
	
	Note that the Google Earth feature requires an architectural redesign anyway so
	this file and the need to dynamically generate it may be OBE'd.
-->

<Folder>
	<open>1</open>
	<name>Link</name>
	<NetworkLink>
		<open>1</open>
   		<name>Data</name>
   		<flyToView>1</flyToView>
   		<Link>
      		<href>http://FILL_IN_GOOGLE_EARTH_SERVLET_HOST/GoogleEarthJSON/kml/setup</href>
   		</Link>
	</NetworkLink>
	<NetworkLink>
   		<name>Updater</name>
   		<flyToView>1</flyToView>
   		<Link>
      		<href>http://FILL_IN_GOOGLE_EARTH_SERVLET_HOST/GoogleEarthJSON/kml/update</href>
      		<refreshMode>onInterval</refreshMode>
      		<refreshInterval>1</refreshInterval>
   		</Link>
	</NetworkLink>
</Folder>
</kml>