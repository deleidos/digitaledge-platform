package com.saic.rtws.commons.model.tmsdb;

/**
 * TMS DB SoftwareRelease DTO object.
 * 
 * @author LUCECU
 *
 */
public class SoftwareRelease {

	private String swVersionId;
	private String description;
	private String releaseNotesUrl;
	
	/**
	 * Default Constructor.
	 */
	public SoftwareRelease(){
		
	}
	
	/**
	 * Set the software version id.
	 * 
	 * @param swVersionId String value
	 */
	public void setSWVersionId(String swVersionId){
		this.swVersionId = swVersionId;
	}
	
	/**
	 * Get the software version id
	 * 
	 * @return String value
	 */
	public String getSWVersionId(){
		return swVersionId;
	}
	
	/**
	 * Set the description.
	 * 
	 * @param description String value
	 */
	public void setDescription(String description){
		this.description = description;
	}
	
	/**
	 * Get the description.
	 * 
	 * @return String value
	 */
	public String getDescription(){
		return description;
	}
	
	/**
	 * Set the release notes url.
	 * 
	 * @param releaseNotesUrl String value
	 */
	public void setReleaseNotesUrl(String releaseNotesUrl){
		this.releaseNotesUrl = releaseNotesUrl;
	}
	
	/**
	 * Get the release notes url.
	 * 
	 * @return String value
	 */
	public String getReleaseNotesUrl(){
		return releaseNotesUrl;
	}
}
