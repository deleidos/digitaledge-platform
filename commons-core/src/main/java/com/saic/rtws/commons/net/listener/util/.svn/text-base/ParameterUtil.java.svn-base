package com.saic.rtws.commons.net.listener.util;

public final class ParameterUtil {

	private ParameterUtil() {
		// Not instantiable.
	}
	
	public static boolean anyNull(Object ... parameters) {
		
		for (Object param : parameters) {
			if (param == null) {
				return true;
			}
		}
		
		return false;
		
	}
	
}