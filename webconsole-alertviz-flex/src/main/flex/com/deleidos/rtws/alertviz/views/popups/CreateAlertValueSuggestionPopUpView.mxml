<?xml version="1.0" encoding="utf-8"?>
<rtws:ResizableTitleWindow
	xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark" 
	xmlns:mx="library://ns.adobe.com/flex/mx"
	xmlns:rtws="com.deleidos.rtws.commons.view.component.*"
	width="350" height="175" 
	title="Error: Incorrect Format">
	
	<fx:Declarations>
		
	</fx:Declarations>
	
	<s:VGroup width="100%" height="100%" horizontalAlign="center">
		<s:TextArea id="suggestionArea" width="100%" height="100%" editable="false" textAlign="center"/>
	</s:VGroup>
	
	<rtws:controlBarContent>
		<s:Button id="yesButton" label="YES" enabled="true"/>
		<s:Button id="noButton" label="NO" enabled="true" />
		<s:Button id="ignoreButton" label="IGNORE" enabled="true" />
	</rtws:controlBarContent>
	
</rtws:ResizableTitleWindow>
