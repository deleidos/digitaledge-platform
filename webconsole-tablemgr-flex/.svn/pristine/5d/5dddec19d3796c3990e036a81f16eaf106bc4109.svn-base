<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
					xmlns:s="library://ns.adobe.com/flex/spark" 
					xmlns:mx="library://ns.adobe.com/flex/mx"
					xmlns:rtws-common="com.deleidos.rtws.commons.view.component.*"
					xmlns:sequence="com.deleidos.rtws.tablemgr.view.db.sequence.*"
					stateChangeComplete="onStateChangeComplete(event);"
					creationComplete="onCreationComplete();">
	
	<fx:Declarations>
		<mx:Validator
			id="simpleValueValidator"
			source="{valueTextInput}"
			property="text"
			triggerEvent="change"
			required="true"
			requiredFieldError="Value is Required"
			enabled="false" />
		
		<s:NumberValidator
			id="intValueValidator"
			source="{valueTextInput}"
			property="text"
			triggerEvent="change"
			required="true"
			requiredFieldError="Value is Required"
			domain="{NumberValidatorDomainType.INT}"
			parseError="Not a valid integer"
			enabled="false" />
		
		<s:NumberValidator
			id="floatValueValidator"
			source="{valueTextInput}"
			property="text"
			triggerEvent="change"
			required="true"
			requiredFieldError="Value is Required"
			domain="{NumberValidatorDomainType.REAL}"
			parseError="Not a valid number"
			enabled="false" />
		
		<mx:Validator
			id="seqSelectionValidator"
			source="{sequenceSelector}"
			property="sequence"
			triggerEvent="sequenceChange"
			required="true"
			requiredFieldError="Sequence is Required"
			enabled="false" />
	</fx:Declarations>
	
	<s:layout>
		<s:VerticalLayout />
	</s:layout>
	
	<s:states>
		<s:State id="invalidState" name="rtws_invalid" stateGroups="rtws_msgGroup" />
		<s:State id="naState" name="rtws_na" stateGroups="rtws_msgGroup" />
		<s:State id="noDefaultValueState" name="rtws_noDefault" stateGroups="rtws_noParamsGroup" />
		<s:State id="nullDefaultValueState" name="rtws_nullDefault" stateGroups="rtws_noParamsGroup" />
		<s:State id="stringDefaultValueState" name="rtws_stringValue" stateGroups="rtws_textEntryValueGroup" />
		<s:State id="intDefaultValueState" name="rtws_intValue" stateGroups="rtws_textEntryValueGroup" />
		<s:State id="floatDefaultValueState" name="rtws_floatValue" stateGroups="rtws_textEntryValueGroup" />
		<s:State id="sequenceDefaultValueState" name="rtws_sequence"
						 enterState="onSeqStateEnter();"
						 exitState="onSeqStateExit();" />
	</s:states>
	
	<s:HGroup width="100%" verticalAlign="baseline">
		<s:Label styleName="formLabel"
						 maxDisplayedLines="1"
						 lineBreak="toFit"
						 width="{_labelColumnWidth}"
						 text="Default Value:"
						 toolTip="Default Value:" />
		
		<s:Label maxDisplayedLines="1"
						 lineBreak="toFit"
						 width="100%"
						 text="{_msgText}"
						 toolTip="{_msgText}"
						 includeIn="rtws_msgGroup" /> 
		
		<s:DropDownList id="defaultValueTypeDropDownList" width="100%"
										dataProvider="{_defaultValueOptions}"
										labelField="displayLabel"
										selectedIndex="{_selectedDefaultValueIndex}"
										change="onDefaultValueModeChange(event);"
										excludeFrom="rtws_msgGroup" />
		
	</s:HGroup>
	
	<s:VGroup width="100%" paddingLeft="0" excludeFrom="rtws_noParamsGroup">
		<s:HGroup width="100%" verticalAlign="middle" includeIn="rtws_textEntryValueGroup">
			<s:RichText styleName="formLabel"
							 maxDisplayedLines="1"
							 lineBreak="toFit"
							 width="{_labelColumnWidth}"
							 textIndent="{_nestedLabelIndentSize}"
							 text="Value:"
							 toolTip="Value"  />
			<s:TextInput id="valueTextInput" width="100%" prompt="Enter a Default Value" text="{_value}" change="onValueChange(event);" />
		</s:HGroup>
		
		<s:HGroup width="100%" verticalAlign="baseline" includeIn="rtws_sequence">
			<s:RichText styleName="formLabel"
							 maxDisplayedLines="1"
							 lineBreak="toFit"
							 width="{_labelColumnWidth}"
							 textIndent="{_nestedLabelIndentSize}"
							 text="Sequence:"
							 toolTip="Sequence:" />
			<sequence:SequenceSelector id="sequenceSelector"
																 tableCtxt="{_tableCtxt}"
																 sequence="{_sequence}"
																 width="100%" height="100%" />
		</s:HGroup>
	</s:VGroup>
	
	<fx:Script>
		<![CDATA[
			import com.deleidos.rtws.commons.event.view.PopupEvent;
			import com.deleidos.rtws.commons.util.StateUtils;
			import com.deleidos.rtws.commons.util.StringUtils;
			import com.deleidos.rtws.commons.util.validation.ValidationUtils;
			import com.deleidos.rtws.tablemgr.model.TableContext;
			import com.deleidos.rtws.tablemgr.model.db.Sequence;
			import com.deleidos.rtws.tablemgr.model.db.defaultvalue.AbsSimpleValueDefaultValue;
			import com.deleidos.rtws.tablemgr.model.db.defaultvalue.CustomSqlDefaultValue;
			import com.deleidos.rtws.tablemgr.model.db.defaultvalue.IDefaultValue;
			import com.deleidos.rtws.tablemgr.model.db.defaultvalue.NoDefaultValue;
			import com.deleidos.rtws.tablemgr.model.db.defaultvalue.NullDefaultValue;
			import com.deleidos.rtws.tablemgr.model.db.defaultvalue.NumericDefaultValue;
			import com.deleidos.rtws.tablemgr.model.db.defaultvalue.SequenceDefaultValue;
			import com.deleidos.rtws.tablemgr.view.db.ColumnDisplayItem;
			
			import mx.binding.utils.ChangeWatcher;
			import mx.collections.ArrayList;
			import mx.events.FlexEvent;
			import mx.validators.IValidator;
			import mx.validators.NumberValidatorDomainType;
			
			import spark.events.IndexChangeEvent;
			import spark.events.TextOperationEvent;
			
			private var _activeValidators:Vector.<IValidator> = new Vector.<IValidator>();
			
			[Bindable] private var _labelColumnWidth:int = 100;
			[Bindable] private var _nestedLabelIndentSize:int = 15;
			[Bindable] private var _msgText:String = "";
			
			private var _columnDisplayItem:ColumnDisplayItem;
			private var _columnDisplayItemWatchedPropNames:Vector.<String> = Vector.<String>(["type", "nullable", "isInPrimaryKey", "isInUniqueConstraint"]);
			private var _columnDisplayItemChangeWatchers:Vector.<ChangeWatcher> = new Vector.<ChangeWatcher>();
			private var _columnDisplayItemChangeWatcher:ChangeWatcher;
			[Bindable] private var _defaultValueOptions:ArrayList = new ArrayList();
			
			[Bindable] private var _defaultValue:IDefaultValue;
			[Bindable] private var _selectedDefaultValueIndex:int = -1;
			[Bindable] private var _value:String = "";
			
			[Bindable] private var _tableCtxt:TableContext = null;
			
			[Bindable] private var _sequence:Sequence = null;
			
			private var _isCreationComplete:Boolean = false;
			private function onCreationComplete():void {
				_isCreationComplete = true;
			}
			
			private function onSeqStateEnter():void {
				if(_isCreationComplete && sequenceSelector && sequenceSelector != null) {
					var createMediatorEvent:PopupEvent = new PopupEvent(PopupEvent.CREATE_MEDIATOR, true);
					createMediatorEvent.view = sequenceSelector;
					dispatchEvent(createMediatorEvent);
				}
			}
			
			private function onSeqStateExit():void {
				if(sequenceSelector && sequenceSelector != null) {
					var disposeMediatorEvent:PopupEvent = new PopupEvent(PopupEvent.DISPOSE_MEDIATOR, true);
					disposeMediatorEvent.view = sequenceSelector;
					dispatchEvent(disposeMediatorEvent);
				}
			}
			
			private function onStateChangeComplete(event:FlexEvent):void {
				if(defaultValueTypeDropDownList && defaultValueTypeDropDownList != null) {
					/*
					 * In some cases, the selectedIndex might be the same numerical value,
					 * so data binding will not fire an event and force a re-selection of
					 * the correct option.  Therefore, we force a set here.
					 */
					defaultValueTypeDropDownList.selectedIndex = _selectedDefaultValueIndex;
				}
				
				cfgValidationForCurrentState();
			}
			
			public function get tableCtxt():TableContext {
				return _tableCtxt;
			}
			
			public function set tableCtxt(value:TableContext):void {
				_tableCtxt = value;
			}
			
			public function set columnDisplayItem(value:ColumnDisplayItem):void {
				if(this._columnDisplayItemChangeWatchers.length > 0) {
					for each(var changeWatcher:ChangeWatcher in _columnDisplayItemChangeWatchers) {
						changeWatcher.unwatch();
					}
					this._columnDisplayItemChangeWatchers.splice(0, _columnDisplayItemChangeWatchers.length);
				}
				
				this._columnDisplayItem = value;
				_defaultValue = (_columnDisplayItem.defaultValue == null ? new NoDefaultValue() : _columnDisplayItem.defaultValue);
				
				if(_defaultValue == null || _defaultValue is NoDefaultValue) {
					_value = "";
					_sequence = null;
				}
				else if(_defaultValue is NullDefaultValue) {
					_value = "";
					_sequence = null;
				}
				else if(_defaultValue is AbsSimpleValueDefaultValue) {
					
					if(_defaultValue is NumericDefaultValue) {
						var tmpValue:Number = Number(_defaultValue.value);
						if(isNaN(tmpValue)) {
							_value = "";
						}
						else {
							_value = tmpValue.toString();
						}
					}
					else {
						_value = _defaultValue.value;
					}
					
					_sequence = null;
				}
				else if(_defaultValue is SequenceDefaultValue) {
					_value = "";
					_sequence = (SequenceDefaultValue(_defaultValue).value as Sequence);
				}
				
				configureViewMode();
				
				if(this._columnDisplayItem != null) {
					for each(var propNameToWatch:String in _columnDisplayItemWatchedPropNames) {
						this._columnDisplayItemChangeWatchers.push(
							ChangeWatcher.watch(_columnDisplayItem, propNameToWatch, 
								onColumnDisplayItemChange, false, true));
					}
				}
			}
			
			private function onColumnDisplayItemChange(event:Event):void {
				configureViewMode();
			}
			
			private function onDefaultValueModeChange(event:IndexChangeEvent):void {
				_selectedDefaultValueIndex = event.newIndex;
				
				if(event.newIndex >= 0 && event.newIndex < _defaultValueOptions.length) {
					_defaultValue = (_defaultValueOptions.getItemAt(event.newIndex) as IDefaultValue);
				}
				
				configureViewMode();
			}
			
			private function onValueChange(event:TextOperationEvent):void {
				_value = valueTextInput.text;
			}
			
			private function configureViewMode():void {
				var newState:State = null;
				
				if(_columnDisplayItem == null || _columnDisplayItem.type == null) {
					if(_columnDisplayItem == null) {
						_msgText = "Invalid Data Provided";
					}
					else {
						_msgText = "Please Select a Data Type First";
					}
					
					_defaultValue = null;
					_defaultValueOptions.removeAll();
					_selectedDefaultValueIndex = -1;
					
					newState = invalidState;
				}
				else {
					var defaultValueOptions:Vector.<IDefaultValue> = _columnDisplayItem.type.dataType.getSupportedDefaultValues(_columnDisplayItem.isInPrimaryKey == false && _columnDisplayItem.isInUniqueConstraint == false && _columnDisplayItem.nullable);
					
					if(defaultValueOptions != null) {
						// Remove Invalid Options
						var indicesToRemove:Array = new Array();
						for(var invalidValueIndex:int = (defaultValueOptions.length - 1); invalidValueIndex >= 0; invalidValueIndex--) {
							if(defaultValueOptions[invalidValueIndex] == null || StringUtils.isBlank(defaultValueOptions[invalidValueIndex].displayLabel) ||
								defaultValueOptions[invalidValueIndex] is CustomSqlDefaultValue) {
								indicesToRemove.push(invalidValueIndex);
							}
						}
						
						for(var removalIndex:int=0; removalIndex < indicesToRemove.length; removalIndex++) {
							defaultValueOptions.splice(indicesToRemove[removalIndex], 1);
						}
					}
					
					if(defaultValueOptions == null || defaultValueOptions.length == 0) {
						_msgText = "N/A";
						
						_defaultValue = null;
						
						_defaultValueOptions.removeAll();
						_selectedDefaultValueIndex = -1;
						
						newState = naState;
					}
					else {
						_msgText = "";
						
						// Locate the best available match for the _defaultValue
						
						var currDefaultValueInDp:Boolean = false;
						var modeMatch:IDefaultValue = null;
						var modeMatchIndex:int = -1;
						
						var index:int=0;
						_defaultValueOptions.removeAll();
						for each(var defaultValueOption:IDefaultValue in defaultValueOptions) {
							_defaultValueOptions.addItem(defaultValueOption);
							
							if(currDefaultValueInDp == false && _defaultValue != null) {
								if(_defaultValue.id == defaultValueOption.id) {
									// The pre-existing default value exists in the currently available options
									currDefaultValueInDp = true;
									_selectedDefaultValueIndex = index;
								}
								else if(_defaultValue.mode == defaultValueOption.mode) {
									modeMatch = defaultValueOption;
									modeMatchIndex = index;
								}
							}
							
							index++;
						}
						
						if(currDefaultValueInDp == false) {
							
							if(modeMatch != null) {
								_defaultValue = modeMatch;
								_selectedDefaultValueIndex = modeMatchIndex;
							}
							else {
								_defaultValue = _defaultValueOptions.getItemAt(0) as IDefaultValue;
								_selectedDefaultValueIndex = 0;
							}
						}
						
						newState = getStateForDefaultValue(_defaultValue);
					}
				}
				
				if(newState.name == currentState) {
					// State didn't change
					
					// Likely that the selected index did not change either
					// Therefore, Data Binding will not fire an event to re-select the drop-down
					if(defaultValueTypeDropDownList && defaultValueTypeDropDownList != null) {
						defaultValueTypeDropDownList.selectedIndex = _selectedDefaultValueIndex;
					}
					
					// Update validators
					cfgValidationForCurrentState();
				}
				else {
					// Change state, selectedIndex, validators will be set up on successful change
					currentState = newState.name;
				}
			}
			
			private function getStateForDefaultValue(defaultValue:IDefaultValue):State {
				var result:State = null;
				
				if(defaultValue == null || defaultValue is NoDefaultValue) {
					result = noDefaultValueState;
				}
				else if(defaultValue is NullDefaultValue) {
					result = nullDefaultValueState;
				}
				else if(defaultValue is AbsSimpleValueDefaultValue) {
					if(defaultValue is NumericDefaultValue) {
						if(NumericDefaultValue(defaultValue).intMode) {
							result = intDefaultValueState;
						}
						else {
							result = floatDefaultValueState;
						}
					}
					else {
						result = stringDefaultValueState;
					}
				}
				else if(defaultValue is SequenceDefaultValue) {
					result = sequenceDefaultValueState;
				}
				
				return result;
			}
			
			public function set labelColumnWidth(value:int):void
			{
				this._labelColumnWidth = value;
			}
			
			public function set nestedLabelIndentSize(value:int):void
			{
				this._nestedLabelIndentSize = value;
			}

			public function getValidators():Vector.<IValidator> {
				var result:Vector.<IValidator> = null;
				
				if(_activeValidators == null) {
					result = new Vector.<IValidator>();
				}
				else {
					result = _activeValidators.concat();
				}
				
				return result;
			}
			
			public function validate():Vector.<String> {
				return ValidationUtils.validateAll(_activeValidators);
			}
			
			public function getColumnDefaultValue():* {
				if(this.currentState == invalidState.name) {
					return Vector.<String>(["Default Value options could not be determined."]);
				}
				
				var errorMsgs:Vector.<String> = validate();
				if(errorMsgs != null)
				{
					return errorMsgs;
				}
				
				if(this.currentState == naState.name || (defaultValueTypeDropDownList.selectedItem as IDefaultValue) == null) {
					return null;
				}
				
				var result:IDefaultValue = (defaultValueTypeDropDownList.selectedItem as IDefaultValue).clone();
				if(StateUtils.isCurrentStateInGroup(this, "rtws_textEntryValueGroup")) {
					try {
						result.value = valueTextInput.text;
					}
					catch(error:Error) {
						return Vector.<String>(["Value is not valid"]);
					}
				}
				else if(currentState == sequenceDefaultValueState.name) {
					try {
						result.value = sequenceSelector.sequence;
					}
					catch(error:Error) {
						return Vector.<String>(["Sequence is not valid"]);
					}
				}
				
				return result;
			}
			
			private function cfgValidationForCurrentState():void {
				_activeValidators.splice(0, _activeValidators.length);
				
				if(StateUtils.isCurrentStateInGroup(this, "rtws_textEntryValueGroup")) {
					if(currentState == stringDefaultValueState.name) {
						simpleValueValidator.enabled = true;
						intValueValidator.enabled = false;
						floatValueValidator.enabled = false;
						seqSelectionValidator.enabled = false;
						
						_activeValidators.push(simpleValueValidator);
					}
					else if(currentState == intDefaultValueState.name) {
						simpleValueValidator.enabled = false;
						intValueValidator.enabled = true;
						floatValueValidator.enabled = false;
						seqSelectionValidator.enabled = false;
						
						_activeValidators.push(intValueValidator);
					}
					else if(currentState == floatDefaultValueState.name) {
						simpleValueValidator.enabled = false;
						intValueValidator.enabled = false;
						floatValueValidator.enabled = true;
						seqSelectionValidator.enabled = false;
						
						_activeValidators.push(floatValueValidator);
					}
					
					if(valueTextInput && valueTextInput != null) {
						// Clear any previous error messages
						valueTextInput.errorString = "";
						
						if(StringUtils.isNotBlank(valueTextInput.text)) {
							validate();
						}
					}
				}
				else if(currentState == sequenceDefaultValueState.name) {
					simpleValueValidator.enabled = false;
					intValueValidator.enabled = false;
					floatValueValidator.enabled = false;
					seqSelectionValidator.enabled = true;
					
					_activeValidators.push(seqSelectionValidator);
				}
				else {
					simpleValueValidator.enabled = false;
					intValueValidator.enabled = false;
					floatValueValidator.enabled = false;
					seqSelectionValidator.enabled = false;
				}
			}
		]]>
	</fx:Script>
</s:Group>
