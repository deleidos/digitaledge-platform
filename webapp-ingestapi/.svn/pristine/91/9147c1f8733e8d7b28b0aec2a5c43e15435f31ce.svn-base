	/** The base directory of the project */
	private String basedir;
	
	/** Used to load in the ingest-ext.jar */
	URL ingestUrl = null;

	/** Used to load in the ingest-ext.jar */
	URL ingestExtUrl = null;
	
	/** Used to load in the transport.jar */
	URL transportUrl = null;
	
	/**
     * Sets up the test fixture. 
     * (Called before every test case method.) 
     */
	@Before
	protected void setUp() {
		basedir = System.getProperty("basedir");
		
		if (basedir == null) {
			System.err.println("Need to set basedir in system properties at startup time");
		}
		
		ServiceLoader loader = LoaderFactory.getServiceLoader();
		List<File> jars = JarUtil.findJarsFromDirectories(basedir + "/src/test/resources");
		loader.load(jars);
		
		File ingestJar = new File(basedir + "/src/test/resources/deleidos-rtws-ingest.jar");
		File extJar = new File(basedir + "/src/test/resources/deleidos-rtws-ingest-ext.jar");
		File transportJar = new File(basedir + "/src/test/resources/deleidos-rtws-transport.jar");
		
		try {
			ingestUrl = ingestJar.toURI().toURL();
			ingestExtUrl = extJar.toURI().toURL();
			transportUrl = transportJar.toURI().toURL();
			System.out.println("ingestExtUrl: " + ingestExtUrl);
		} catch (MalformedURLException e) {
			e.printStackTrace();
		}

	}
	
	/**
     * Tears down the test fixture. 
     * (Called after every test case method.) 
     */ 
	@After
	protected void tearDown() {
		
	}
	
	/**
	 * Test creating a service with errors.
	 */
	@Test
	public void testCreateServiceWithError() {
		// Test bogus service type
		
		try {
			ServiceFactoryUtils.createService("com.deleidos.rtws.ext.parser.ConfigurableCsvParser", 
					ingestExtUrl, ServiceType.valueOf("BOGUS_TYPE"));
			
			fail(); // should not get here!
		} catch (Exception ex) {
			assertTrue(ex instanceof IllegalArgumentException);
		}
		
		// Test class cannot be found
		
		try {
			Service result = ServiceFactoryUtils.createService("com.deleidos.rtws.ext.parser.BogusParser", 
					ingestExtUrl, ServiceType.PARSER);
			
			assertNull(result);
		} catch (Exception ex) {
			fail();
		} 
		
		// Test attempting to create a service with a non class type
		
		try {
			Service result = ServiceFactoryUtils.createService("README", ingestExtUrl, ServiceType.PROCESSOR);
			
			assertNull(result);
		} catch (Exception ex) {
			fail();
		}
		
	}
	
