		private var mappingTable:Array;
		
		public function SimulatedTmsDatabaseServices():void {
			mappingTable = new Array();			
			mappingTable["AVAILABILITY_ZONES"]   = availabilityZonesJson;
			mappingTable["ACCOUNT_SYSTEMS"]      = accountSystemsJson;
			mappingTable["DATASINK_CONFIG"]      = datasinkConfigJson;
			mappingTable["IAAS_ACCOUNTS"]        = iaasAccountsJson;
			mappingTable["PROCESS_GROUP_CONFIG"] = processGroupConfigJson;
			mappingTable["SOFTWARE_RELEASES"]    = softwareReleasesJson;
			mappingTable["SYSTEM_SIZING"]        = systemSizingJson;
			mappingTable["TENANT_ACCOUNT_ACCESS"]= tenantAccountAccessJson;
			mappingTable["TRANSPORT_CONFIG"]     = transportConfigJson;
			mappingTable["WEBAPP_CONFIG"]        = webappConfigJson;
		}
		
		public function loadTable(dao:DatabaseTable,
								  completionEvent:Event,
								  failureEvent:Event,
								  expression:String=null):void
		{
			// DBAPI URL: dbapi/json/select/auth-tms.rtsaic.com/application/<table>/column1,column2,...
			
			var json:String = mappingTable[dao.tableName];
			if (json == null) {
				throw new Error("Table has no test data: " + dao.tableName );
			}
			
			if ((dao.tableName == "ACCOUNT_SYSTEMS") || (dao.tableName == "IAAS_ACCOUNTS")) {
				dao.loadTableFromJsonNew(json);
			}
			else {
				dao.loadTableFromJson(json);
			}
			
			dispatch(completionEvent);
		}
		
		public function domainDupCheck(sysPropModel:SystemPropertiesModel, completionEvent:Event):void
		{
			sysPropModel.setDomainDuplicate("{\"count\":0}";
			dispatch(completionEvent);
		}
	}
