{
	//Fields that are part of the common data model
	"eventType"		: "string",
	"objectType"	: "string",
	"objectIdName"	: "string",
	"objectIdValue"	: "string",
	"NumGeos"       : "number",
	"geoList"		: 
	[{
		"geoTime"	: "datestring MM/dd/yy HH:mm:ss",
		"geoShape"	: 
		{
			"type"		: "number",
			"shapeKey"	: "string",
			"latitude"	: "number",
			"longitude"	: "number",
			"param1"	: "number",
			"param2"	: "number",
			"param3"	: "number"
		},
		"geoGrid"	: "enrichment type=mgrs_grid parameters=geoList[].geoShape"
	}],
	
	"markerImage"	: "string",
	"NumContent"    : "number",
    "Content"       :
    [{
        "contentName"   : "string",
        "mimeType"      : "string",
        "contentThumb"  : "string",
        "isURI"         : "string",
        "contentValue"  : "string"
    }],
	
	//Extrinsic Data fields specific to each input, each listed separately
	"extrinsic_Checkpoint"	: 
	{
		"plateNum"	: "string",
		"date"		: "string",
		"time"		: "string",
		"country"	: "string",
		"cityState"	: "string",
		"latitude"	: "number",
		"longitude"	: "number"
	},
	
	"extrinsic_RFEvent"	: 
	{
		"towerId"	: "string",
		"date"		: "string",
		"time"		: "string",
		"latitude"	: "number",
		"longitude"	: "number",
		"bearing"	: "number",
		"frequency"	: "string",
		"channel"	: "string",
		"modulation": "string"
	},
	
	"extrinsic_IDTracking"	: 
	{
		"id"		: "string",
		"name"		: "enrichment type=dimension_table table=IDTRACKING parameters=extrinsic_IDTracking.id extractPath=name",
		"cartype"	: "enrichment type=dimension_table table=IDTRACKING parameters=extrinsic_IDTracking.id extractPath=cartype",
		"date"		: "string",
		"time"		: "string",
		"latitude"	: "number",
		"longitude"	: "number"
	}
}
		