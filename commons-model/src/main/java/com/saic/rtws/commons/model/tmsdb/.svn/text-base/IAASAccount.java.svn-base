package com.saic.rtws.commons.model.tmsdb;

import java.sql.Timestamp;

/**
 * TMS DB IAASAccount DTO object.
 * 
 * @author LUCECU
 *
 */
public class IAASAccount {
	private int accountId;
	private String accountName;
	private String description;
	private String iaasServiceName;
	private String keypairName;
	private String iaasCredentials;
	private String anchorInstanceId;
	private String accountEmail;
	private String iaasServiceNumber;
	private String domainNameSuffix;
	private Timestamp eulaAcceptedTimestamp;
	private Timestamp licenseExpirationTimestamp;
	
	/**
	 * Default constructor.
	 */
	public IAASAccount(){
		
	}
	
	/**
	 * Set the account id.
	 * 
	 * @param accountId int value
	 */
	public void setAccountId(int accountId){
		this.accountId = accountId;
	}
	
	/**
	 * Get the account id.
	 * 
	 * @return int value
	 */
	public int getAccountId(){
		return accountId;
	}
	
	/**
	 * Set the account name.
	 * 
	 * @param accountName String value
	 */
	public void setAccountName(String accountName){
		this.accountName = accountName;
	}
	
	/**
	 * Get the account name.
	 * 
	 * @return String value
	 */
	public String getAccountName(){
		return accountName;
	}
	
	/**
	 * Set the description.
	 * 
	 * @param description String value
	 */
	public void setDescription(String description){
		this.description = description;
	}
	
	/**
	 * Get the description.
	 * 
	 * @return String value
	 */
	public String getDescription(){
		return description;
	}
	
	/**
	 * Set the iaas service name.
	 * 
	 * @param iaasServiceName String value
	 */
	public void setIaasServiceName(String iaasServiceName){
		this.iaasServiceName = iaasServiceName;
	}
	
	/**
	 * Get the iaas service name.
	 * 
	 * @return String value
	 */
	public String getIaasServiceName(){
		return iaasServiceName;
	}
	
	/**
	 * Set the key pair name. 
	 * 
	 * @param keypairName String value
	 */
	public void setKeypairName(String keypairName){
		this.keypairName = keypairName;
	}
	
	/**
	 * Get the key pair name.
	 * 
	 * @return String value
	 */
	public String getKeypairName(){
		return keypairName;
	}
	
	/**
	 * Set the iaas credentials.
	 * 
	 * @param iaasCredentials String value
	 */
	public void setIaasCredentials(String iaasCredentials){
		this.iaasCredentials = iaasCredentials;
	}
	
	/**
	 * Get the iaas credentials.
	 * 
	 * @return String value
	 */
	public String getIaasCredentials(){
		return iaasCredentials;
	}
	
	/**
	 * Set the anchor instance.
	 * 
	 * @param anchorInstance String value
	 */
	public void setAnchorInstanceId(String anchorInstanceId) {
		this.anchorInstanceId = anchorInstanceId;
	}
	
	/**
	 * Get the anchor instance.
	 * 
	 * @return String value
	 */
	public String getAnchorInstanceId() {
		return anchorInstanceId;
	}
	
	/**
	 * Set the account email.
	 * 
	 * @param accountEmail String value
	 */
	public void setAccountEmail(String accountEmail) {
		this.accountEmail = accountEmail;
	}
	
	/**
	 * Get the account email.
	 * 
	 * @return String value
	 */
	public String getAccountEmail() {
		return accountEmail;
	}
	
	/**
	 * Set the iaas service number.
	 * 
	 * @param accountEmail String value
	 */
	public void setIaasServiceNumber(String iaasServiceNumber) {
		this.iaasServiceNumber = iaasServiceNumber;
	}
	
	/**
	 * Get the iaas service number.
	 * 
	 * @return String value
	 */
	public String getIaasServiceNumber() {
		return iaasServiceNumber;
	}
	
	/**
	 * Set the domainNameSuffix value.
	 * 
	 * @param domainNameSuffix String value
	 */
	public void setDomainNameSuffix(String domainNameSuffix){
		this.domainNameSuffix = domainNameSuffix;
	}
	
	/**
	 * Get the domainNameSuffix value.
	 * 
	 * @return String value
	 */
	public String getDomainNameSuffix(){
		return domainNameSuffix;
	}
	
	/**
	 * Get the eulaAcceptedTimestamp value.
	 * 
	 * @return String value
	 */
	public Timestamp getEulaAcceptedTimestamp() {
		return eulaAcceptedTimestamp;
	}

	/**
	 * Set the eulaAcceptedTimestamp value.
	 * 
	 * @param eulaAcceptedTimestamp String value
	 */
	public void setEulaAcceptedTimestamp(Timestamp eulaAcceptedTimestamp) {
		this.eulaAcceptedTimestamp = eulaAcceptedTimestamp;
	}

	/**
	 * Get the licenseExpirationTimestamp value.
	 * 
	 * @return String value
	 */
	public Timestamp getLicenseExpirationTimestamp() {
		return licenseExpirationTimestamp;
	}

	/**
	 * Set the licenseExpirationTimestamp value.
	 * 
	 * @param licenseExpirationTimestamp String value
	 */
	public void setLicenseExpirationTimestamp(Timestamp licenseExpirationTimestamp) {
		this.licenseExpirationTimestamp = licenseExpirationTimestamp;
	}
	
	/**
	 * Converts the object to a string format.
	 */
	public String toString(){
		String args = "account id:  %d\ndescription:  %s\niaasServiceName:  %s\nkeypairName:  %s\niaasCredentials:  %s\nanchorInstance:  %s\naccountEmail:  %s\niaasServiceNumber:  %s\ndomainNameSuffix:  %s\neulaAcceptedTimestamp: %s\nlicenseExpirationTimestamp: %s\n";
		String account = String.format(args, accountId, description, iaasServiceName, keypairName, iaasCredentials, anchorInstanceId, accountEmail, iaasServiceNumber, domainNameSuffix, eulaAcceptedTimestamp, licenseExpirationTimestamp);
		
		return account;
	}
	
}
