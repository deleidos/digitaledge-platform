package com.deleidos.rtws.alertviz.expandgraph.graph.properties
{

	public class GraphProperties
	{
		
		private var _name:String;
		public function get name():String{
			return _name;
		}
		public function set name(name:String):void{
			_name = name;
		}
		
		private var _nodeProperties:Properties;
		public function get nodeProperties():Properties{
			return _nodeProperties;
		}
		public function set nodeProperties(nodeProperties:Properties):void{
			_nodeProperties = nodeProperties;
		}
		
		private var _edgeProperties:Properties;
		public function get edgeProperties():Properties{
			return _edgeProperties;
		}
		public function set edgeProperties(edgeProperties:Properties):void{
			_edgeProperties = edgeProperties;
		}
		
		public function GraphProperties(name:String, nodeProps:Properties, edgeProps:Properties)
		{
			_name = name;
			_nodeProperties = nodeProps;
			_edgeProperties = edgeProps;
		}
	}
}