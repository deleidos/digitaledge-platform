package com.saic.rtws.commons.exception;

/** Exception for instance of permission denied.
 */
public class DefaultConfigurationException extends Exception {

	/** Unique serial version. */
	private static final long serialVersionUID = -4838630834264636049L;

	/** 
	 * Constructor. 
	 * @param message Message to be returned
	 */
	public DefaultConfigurationException(final String message) {
		super(message);
	}
	
	/** 
	 * Constructor. 
	 * @param message Message to be returned
	 * @param cause Exception object
	 */
	public DefaultConfigurationException(final String message, final Exception cause) {
		super(message, cause);
	}
}