	public class DataModelEditorContext extends Context
	{
		public function DataModelEditorContext(contextView:DisplayObjectContainer = null, autoStartup:Boolean = true)
		{
			super(contextView, autoStartup);
		}
		
		override public function startup():void
		{
			// Inject model references as singletons
			injector.mapSingletonOf(IAppPropertiesModel, TmsAppPropertiesModel);
			injector.mapSingletonOf(IUser, User);
			injector.mapSingleton(InputModel);
			injector.mapSingleton(DataModelDirectory);
			injector.mapSingleton(EnrichmentDirectory);
			injector.mapSingleton(ParserDirectory);
			injector.mapSingleton(EnrichmentModel);
			injector.mapSingleton(AppParameterModel);
			injector.mapSingleton(RepositoryRestService);
			injector.mapSingleton(CustomEditorList);
			injector.mapSingleton(ScriptDirectory);
			injector.mapSingleton(DataModelUsageModel);
			
			// Inject service as singletons of an interface
			injector.mapClass(IPropertiesService, PropertiesService);
			//injector.mapSingletonOf(IUserServices, MockUserServices);
			injector.mapSingletonOf(IUserServices, UserServices);
			
			injector.mapClass(IThemeService, ThemeService);
			
			//injector.mapSingletonOf(IDataModelServices, SimulatedDataModelServices);
			injector.mapSingletonOf(IDataModelServices, DataModelServices);

			//injector.mapSingletonOf(IEnrichmentServices, SimulatedEnrichmentServices);
			injector.mapSingletonOf(IEnrichmentServices, EnrichmentServices);

			//injector.mapSingletonOf(IParserServices, SimulatedParserServices);
			injector.mapSingletonOf(IParserServices, ParserServices);
			
			injector.mapSingletonOf(IScriptServices, ScriptServices);
			
			injector.mapSingletonOf(ITenantServices, TenantServices);
			
			injector.mapSingletonOf(IDataModelUsagePollingService, DataModelUsagePollingService);
			
			injector.mapSingletonOf(IDataModelWizard, DataModelWizard);
			
			// Map views to mediators
			// mediatorMap.mapView(MainMenuView,   	MainMenuViewMediator);
			mediatorMap.mapView(UserDisplay, UserDisplayMediator, IUserDisplay);
			mediatorMap.mapView(AboutDialog, AboutDialogMediator, null, false, false);
			mediatorMap.mapView(InputModelView,		InputModelViewMediator);
			mediatorMap.mapView(DataSourceMapView,	DataSourceMapViewMediator);
			mediatorMap.mapView(EnrichmentView,		EnrichmentViewMediator);

			// Popups must have auto-mediation turned off - the mediator must be created
			// manually at popup creation time
			mediatorMap.mapView(OpenDataModelPopup, OpenDataModelPopupMediator, null, false, false);
			mediatorMap.mapView(OpenOrNewPopup,		OpenOrNewPopupMediator,     null, false, false);
			
			mediatorMap.mapView(DataModelEditor, DataModelEditorMediator);
			
			// Startup Commands
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_PARSER_DIRECTORY, StartupLoadParserDirectoryCommand, DataLoadOperationEvent);
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_SCRIPT_DIRECTORY, StartupLoadScriptDirectoryCommand, DataLoadOperationEvent);
			commandMap.mapEvent(DataLoadOperationEvent.LOAD_ENRICH_DIRECTORY, StartupLoadEnrichmentDirectoryCommand, DataLoadOperationEvent);
			
			// Map events to commands
			commandMap.mapEvent(PropertiesEvent.FETCH_REMOTE_PROPS_REQUEST, LoadRemotePropsIntoModelCommand);
			commandMap.mapEvent(ClientSessionTimerEvent.RESTART_SESSION_TIMER, RestartClientSessionTimerCommand);
			
			commandMap.mapEvent(UserOpRequestEvent.LOAD_USER_INFO, UserOpRequestCommand);
			commandMap.mapEvent(ThemeOpRequestEvent.LOAD_THEME, ThemeOpRequestCommand);
			commandMap.mapEvent(PopupEvent.OPEN_MODEL, OpenModelDialogCommand, PopupEvent);
			commandMap.mapEvent(ModelServiceEvent.LOAD_MODEL, LoadModelCommand, ModelServiceEvent);
			commandMap.mapEvent(ModelServiceEvent.SAVE_MODEL, SaveModelCommand, ModelServiceEvent);
			commandMap.mapEvent(ModelServiceEvent.DELETE_MODEL, DeleteModelCommand, ModelServiceEvent);
			commandMap.mapEvent(ModelServiceEvent.DM_DELETION_CHECK, DataModelDeleteCheckCommand, ModelServiceEvent);
			commandMap.mapEvent(ModelServiceEvent.ENRICH_DIR_LOAD, LoadEnrichDirectoryCommand, ModelServiceEvent);
			commandMap.mapEvent(ModelServiceEvent.PARSER_DIR_LOAD, LoadParserDirectoryCommand, ModelServiceEvent);
			commandMap.mapEvent(ModelServiceEvent.NEW_MODEL, NewModelCommand, ModelServiceEvent);
			commandMap.mapEvent(ModelServiceEvent.LOAD_MODEL_DIR, LoadDataModelDirectoryCommand, ModelServiceEvent);
			commandMap.mapEvent(ModelServiceEvent.DM_USAGE_CHECK, DataModelUsageCheckCommand, ModelServiceEvent);
			commandMap.mapEvent(ModelServiceEvent.RESET_DM_USAGE_POLLING, ResetDmUsagePollingCommand, ModelServiceEvent);
			commandMap.mapEvent(ModelServiceEvent.START_DM_USAGE_POLLING, StartDmUsagePollingCommand, ModelServiceEvent);
			commandMap.mapEvent(InputModelEditEvent.MODEL_INSERT_FIELD, InsertFieldCommand, InputModelEditEvent);
			commandMap.mapEvent(InputModelEditEvent.MODEL_INSERT_OBJECT, InsertObjectCommand, InputModelEditEvent);
			commandMap.mapEvent(InputModelEditEvent.MODEL_INSERT_ARRAY, InsertArrayCommand, InputModelEditEvent);
			commandMap.mapEvent(InputModelEditEvent.MODEL_DELETE_FIELD, DeleteFieldCommand, InputModelEditEvent);
			commandMap.mapEvent(InputModelEditEvent.MODEL_UPDATE_FIELD, UpdateFieldCommand, InputModelEditEvent);
			commandMap.mapEvent(InputModelEditEvent.MODEL_MAP_ADD_ELEMENT, MapAddArrayElementCommand, InputModelEditEvent);
			commandMap.mapEvent(InputModelEditEvent.MODEL_MAP_DEL_ELEMENT, MapDelArrayElementCommand, InputModelEditEvent);
			commandMap.mapEvent(InputModelEditEvent.MODEL_MAP_GET_FIELD, MapGetFieldCommand, InputModelEditEvent);
			commandMap.mapEvent(InputModelEditEvent.MODEL_MAP_GET_ARRAY, MapGetArrayCommand, InputModelEditEvent);
			commandMap.mapEvent(InputModelEditEvent.MODEL_MAP_USE_CONST, MapUseConstantCommand, InputModelEditEvent);
			commandMap.mapEvent(InputModelEditEvent.MODEL_MAP_CONV_DATE, MapConvertDateCommand, InputModelEditEvent);
			commandMap.mapEvent(InputModelEditEvent.MODEL_MAP_USE_SCRIPT, MapUseScriptCommand, InputModelEditEvent);
			commandMap.mapEvent(InputModelEditEvent.MODEL_ADD_DATASRC,  AddDataSourceCommand, InputModelEditEvent);
			commandMap.mapEvent(InputModelEditEvent.MODEL_EDIT_DATASRC, EditDataSourceCommand, InputModelEditEvent);
			commandMap.mapEvent(InputModelEditEvent.MODEL_REMOVE_DATASRC, RemoveDataSourceCommand, InputModelEditEvent);
			commandMap.mapEvent(EnrichModelEditEvent.MODEL_INSERT_ENRICH, InsertEnrichCommand, EnrichModelEditEvent);
			commandMap.mapEvent(EnrichModelEditEvent.MODEL_REMOVE_ENRICH, DeleteEnrichCommand, EnrichModelEditEvent);
			commandMap.mapEvent(EnrichModelEditEvent.MODEL_MOVE_ENRICH_UP, MoveEnrichUpCommand, EnrichModelEditEvent);
			commandMap.mapEvent(EnrichModelEditEvent.MODEL_MOVE_ENRICH_DOWN, MoveEnrichDownCommand, EnrichModelEditEvent);
			commandMap.mapEvent(EnrichModelEditEvent.MODEL_UPDATE_ENRICH, UpdateEnrichCommand, EnrichModelEditEvent);
			commandMap.mapEvent(ModelServiceEvent.MODEL_WIZARD_START, StartCreateModelCommand, ModelServiceEvent);
			commandMap.mapEvent(ModelServiceEvent.MODEL_WIZARD_POLL, StartProgressPollingCommand, ModelServiceEvent);
			commandMap.mapEvent(ModelServiceEvent.MODEL_WIZARD_LOAD, LoadModelFromWizardCommand, ModelServiceEvent);
			commandMap.mapEvent(ModelServiceEvent.MODEL_WIZARD_GET_DETAIL, GetModelInformationCommand, ModelServiceEvent);
			
			commandMap.mapEvent(ContextEvent.STARTUP_COMPLETE, StartupCommand, ContextEvent, true);
			
			super.startup();
		}
	}
