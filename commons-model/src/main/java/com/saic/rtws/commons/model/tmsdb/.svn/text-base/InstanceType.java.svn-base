package com.saic.rtws.commons.model.tmsdb;

/**
 * TMS DB InstanceType DTO object.
 * 
 * @author LUCECU
 *
 */
public class InstanceType {

	private String instanceType;
	private int numBits;
	private int numCores;
	private int memoryMB;
	
	/**
	 * Default constructor.
	 */
	public InstanceType(){
		
	}
	
	/**
	 * Set instance type.
	 * 
	 * @param instanceType String value
	 */
	public void setInstanceType(String instanceType){
		this.instanceType = instanceType;
	}
	
	/**
	 * Get instance type.
	 * 
	 * @return String value.
	 */
	public String getInstanceType(){
		return instanceType;
	}
	
	/**
	 * Set number of bits for instance.
	 * 
	 * @param numBits int value
	 */
	public void setNumBits(int numBits){
		this.numBits = numBits;
	}
	
	/**
	 * Get number of bits for instance.
	 * 
	 * @return String value
	 */
	public int getNumBits(){
		return numBits;
	}
	
	/**
	 * Set the number of cores for the instance.
	 * 
	 * @param numCores int value
	 */
	public void setNumCores(int numCores){
		this.numCores = numCores;
	}
	
	/**
	 * Get the number of cores for the instance.
	 * 
	 * @return int value
	 */
	public int getNumCores(){
		return numCores;
	}
	
	/**
	 * Set the memory size in MB for the instance.
	 * 
	 * @param memoryMB int value
	 */
	public void setMemoryMB(int memoryMB){
		this.memoryMB = memoryMB;
	}
	
	/**
	 * Get the memory size in MB for the instance.
	 * 
	 * @return int value
	 */
	public int getMemoryMB(){
		return memoryMB;
	}
}
