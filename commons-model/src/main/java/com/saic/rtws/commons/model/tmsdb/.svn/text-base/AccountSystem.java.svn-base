package com.saic.rtws.commons.model.tmsdb;

import javax.xml.bind.annotation.XmlRootElement;

/**
 * TMS DB AccountSystem DTO object.
 * 
 * @author LUCECU
 *
 */
@XmlRootElement
public class AccountSystem {

	private int accountId;
	private String systemDomain;
	private String swVersionId;
	private String configVersionId;
	private String masterInstanceId;
	private String masterAddress;
	private String masterUserData;
	private String systemCredentials;
	private String masterRegion;
	private String masterAZone;
	
	/**
	 * Default constructor.
	 */
	public AccountSystem() { }
	
	/**
	 * Set the account id.
	 * 
	 * @param accountId int value
	 */
	public void setAccountId(int accountId){
		this.accountId = accountId;
	}
	
	/**
	 * Get the account id
	 * 
	 * @return account id int value
	 */
	public int getAccountId(){
		return accountId;
	}
	
	/**
	 * Set the system domain.
	 * 
	 * @param systemDomain string value
	 */
	public void setSystemDomain(String systemDomain){
		this.systemDomain = systemDomain;
	}
	
	/**
	 * Get the system domain.
	 * 
	 * @return string value
	 */
	public String getSystemDomain(){
		return systemDomain;
	}
	
	/**
	 * Set the software version id.
	 * 
	 * @param swVersionId string value
	 */
	public void setSwVersionId(String swVersionId){
		this.swVersionId = swVersionId;
	}
	
	/**
	 * Get the software version id.
	 * 
	 * @return string value
	 */
	public String getSwVersionId(){
		return swVersionId;
	}
	
	/**
	 * Set the configuration version id.
	 * 
	 * @param configVersionId string value
	 */
	public void setConfigVersionId(String configVersionId){
		this.configVersionId = configVersionId;
	}
	
	/**
	 * Get the configuration version id
	 * 
	 * @return string value.
	 */
	public String getConfigVersionId(){
		return configVersionId;
	}
	
	/**
	 * Set the master instance id value.
	 * 
	 * @param masterInstanceId string value.
	 */
	public void setMasterInstanceId(String masterInstanceId) {
		this.masterInstanceId = masterInstanceId;
	}
	
	/**
	 * Get the master instance id value.
	 * 
	 * @return string value.
	 */
	public String getMasterInstanceId() {
		return this.masterInstanceId;
	}
	
	/**
	 * Set the master address value.
	 * 
	 * @param masterAddress string value.
	 */
	public void setMasterAddress(String masterAddress){
		this.masterAddress = masterAddress;
	}
	
	/**
	 * Get the master address value.
	 * 
	 * @return string value.
	 */
	public String getMasterAddress(){
		return masterAddress;
	}
	
	/**
	 * Set the master user data value.
	 * 
	 * @param masterUserData String value
	 */
	public void setMasterUserData(String masterUserData){
		this.masterUserData = masterUserData;
	}
	
	/**
	 * Get the master user data value.
	 * 
	 * @return string value
	 */
	public String getMasterUserData(){
		return masterUserData;
	}
	
	/**
	 * Set the system credentials.
	 * 
	 * @param systemCredentials string value
	 */
	public void setSystemCredentials(String systemCredentials){
		this.systemCredentials = systemCredentials;
	}
	
	/**
	 * Get the system credentials value.
	 * 
	 * @return string value
	 */
	public String getSystemCredentials(){
		return systemCredentials;
	}
	
	/**
	 * Set the master region.
	 * 
	 * @param masterRegion string value
	 */
	public void setMasterRegion(String masterRegion) {
		this.masterRegion = masterRegion;
	}
	
	/**
	 * Get the master region.
	 * 
	 * @return string value
	 */
	public String getMasterRegion() {
		return this.masterRegion;
	}
	
	/**
	 * Set the master availability zone.
	 * 
	 * @param masterAZone string value
	 */
	public void setMasterAZone(String masterAZone) {
		this.masterAZone = masterAZone;
	}
	
	/**
	 * Get the master availability zone.
	 * 
	 * @return string value
	 */
	public String getMasterAZone() {
		return this.masterAZone;
	}
}
