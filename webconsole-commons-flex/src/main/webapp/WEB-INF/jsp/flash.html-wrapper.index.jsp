    <head>
        <title>@html-wrapper.title@</title>
        
        
        <link rel="icon" type="image/png" href="<c:url value="/img/common/deFavicon-32x32.png" />"  />
        
        <meta name="google" value="notranslate" />         
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <!-- Include CSS to eliminate any default margins/padding and set the height of the html element and 
             the body element to 100%, because Firefox, or any Gecko based browser, interprets percentage as 
             the percentage of the height of its parent container, which has to be set explicitly.  Fix for
             Firefox 3.6 focus border issues.  Initially, don't display flashContent div so it won't show 
             if JavaScript disabled.
        -->
        <style type="text/css" media="screen"> 
            html, body  { height:@html-wrapper.height@; }
            body { margin:0; padding:0; overflow:auto; text-align:center; 
                   background-color: white; }   
            object:focus { outline:none; }
            #flashContent { display:none; }
        </style>
        
        <script type="text/javascript" src="<c:url  value="/js/jquery-1.7.2.min.js" />"></script>
        
        <!-- Enable Browser History by replacing useBrowserHistory tokens with two hyphens -->
        <!-- BEGIN Browser History required section >
        <link rel="stylesheet" type="text/css" href="history/history.css" />
        <script type="text/javascript" src="history/history.js"></script>
        <! END Browser History required section -->  
            
        <c:if test="${flexAjaxBridgeName != null}">
					<script type="text/javascript" src="FABridge.js"></script>
        </c:if>
        
        <script type="text/javascript" src="swfobject.js"></script>
        <script type="text/javascript">
            // For version detection, set to min. required Flash Player version, or 0 (or 0.0.0), for no version detection. 
            var swfVersionStr = "@html-wrapper.version@";
            // To use express install, set to playerProductInstall.swf, otherwise the empty string. 
            var xiSwfUrlStr = "";
            var flashvars = {};
            <%=setFlashVarsJavascript%>
            var params = {};
            params.quality = "high";
            
           	// Setting the window mode to allow the timer dialog to be raised over the swf
            if(jQuery.browser.msie)
           	{
            	// IE allows mouse wheel scrolling and layering in this option
           		params.wmode = "opaque";
           	}
            else if(jQuery.browser.webkit)
           	{
           		// No Setting in Chrome that allows layering and scrolling.  Siding with layering for the time being
            	params.wmode = "opaque";
           	}
            else if(jQuery.browser.mozilla == false)
            {
           		// FF supports layering and scrolling with no explicit setting.  Set to opaque in all other cases
           		params.wmode = "opaque";
            }
            
            params.bgcolor = "white";
            params.allowscriptaccess = "sameDomain";
            params.allowfullscreen = "true";
            var attributes = {};
            attributes.id = "App";
            attributes.name = "App";
            attributes.align = "middle";
            swfobject.embedSWF(
                "@html-wrapper.swf@", "flashContent", 
                "@html-wrapper.width@", "@html-wrapper.height@", 
                swfVersionStr, xiSwfUrlStr, 
                flashvars, params, attributes);
            // JavaScript enabled so display the flashContent div in case it is not replaced with a swf object.
            swfobject.createCSS("#flashContent", "display:block;text-align:left;");
        </script>
        
			<link type="text/css" href="<c:url value="/css/redmond/jquery-ui-1.8.21.css" />" rel="Stylesheet" />
			<script type="text/javascript" src="<c:url value="/js/jquery-ui-1.8.21.min.js" />"></script>
			
			<script type="text/javascript" src="<c:url value="/js/bootstrap-2.1.0.min.js" />"></script>
			<link type="text/css" href="<c:url value="/css/bootstrap-2.1.0.min.css" />" rel="Stylesheet" />
			
			<script type="text/javascript" src="<c:url value="/js/com/deleidos/rtws/jscommons/core/sessiontimeout/SessionTimer.js" />"></script>
			<script type="text/javascript">
				$.noConflict();
				
				var com = com || {};
				com.deleidos = com.deleidos || {};
				com.deleidos.rtws = com.deleidos.rtws || {};
				com.deleidos.rtws.jscommons = com.deleidos.rtws.jscommons || {};
				
				com.deleidos.rtws.jscommons.utils = com.deleidos.rtws.jscommons.utils || {};
				com.deleidos.rtws.jscommons.utils.ui = com.deleidos.rtws.jscommons.utils.ui || {};
				com.deleidos.rtws.jscommons.utils.ui.openDialog = function(url, dialogOptions){
					if(url)
					{
						// NICETOHAVE: Allow an id to be passed to force a single dialog
						
						var tmpDivId = null;
						
						var $tmpDiv = null;
						while($tmpDiv == null)
						{
							tmpDivId = "tmp_" + new Date().getTime();
							var $existingDivs = jQuery("div#" + tmpDivId);
							if($existingDivs.length == 0)
							{
								$tmpDiv = jQuery("<div id='" + tmpDivId + "'></div>");
						  	$tmpDiv.hide();
						  	jQuery("body").append($tmpDiv);
							}
						}
						
						$tmpDiv.load(url, null, function() {
					  	 var $dialog = $tmpDiv.dialog(dialogOptions);
					  	 $dialog.one("dialogclose", function() {
					  		 jQuery(this).dialog('destroy').remove();
					  	 });
					   });
					}
				};
				
				com.deleidos.rtws.jscommons.core = com.deleidos.rtws.jscommons.core || {};
				com.deleidos.rtws.jscommons.core.sessiontimeout = com.deleidos.rtws.jscommons.core.sessiontimeout || {};
				
				com.deleidos.rtws.jscommons.core.sessiontimeout.impl = function(){
					var sessionTimer = null;
					var $timeoutWarningDialog = null;
					var $sessionTimeoutDialog = null;
					var $ssoCommsLostDialog = null;
					var $ssoLogoutDialog = null;
					
					function initSessionTimer()
					{
						var sessionSeconds = <c:out value="${pageContext.session.maxInactiveInterval}" default="-1" />;
						
						if(sessionSeconds > 0)
						{
							// Timeout Dialog
							$timeoutWarningDialog = jQuery("#timeoutWarningDialog").dialog({
								modal: true,
								width: 550,
								height: "auto",
								autoOpen: false,
								buttons: [
									{
										text: "Keep Working",
										click: function() {closeWarningDialog(true);}
								  },
								  {
										text: "Sign Out",
										click: function() {closeWarningDialog(false);}
								  }
								],
								close: function() {closeWarningDialog();}
							});
							
							// Session Expiration Dialogs (Timeout and SSO)
							var $sessionExpTemplate = jQuery("div.sessionExpirationTemplate");
							
							var isWindowCloseable = ((window && window.opener) ? true : false);
							
							// Toggle options based on whether the window can be closed with javascript or not
							$sessionExpTemplate.find(".closeActionDesc").toggle(isWindowCloseable);
							$sessionExpTemplate.find(".signOutActionDesc").toggle(!isWindowCloseable);
							
							$sessionExpTemplate.find(".instructionToggle").click(function(event){
								var $toggleElem = jQuery(this);
								var $templateRoot = $toggleElem.closest("div.sessionExpirationTemplate");
								var $instructions = $templateRoot.find(".instructions");
								
								jQuery(this).closest("div.sessionExpirationTemplate").find(".instructions").slideToggle("fast", function () {
									if($instructions.css("display") === "none") {
										$toggleElem.find(".collapseIcon").hide();
										$toggleElem.find(".expandIcon").show();									
									}
									else {
										$toggleElem.find(".collapseIcon").show();
										$toggleElem.find(".expandIcon").hide();
										
										if(event && event.target && window) {
											var $dialog = jQuery(event.target).closest(".ui-dialog");
											
											if($dialog) {
												var position = $dialog.offset();
												var height = $dialog.height();
												var windowHeight = jQuery(window).height();
												if(position.top + height > windowHeight) {
													$dialog.position({
														my: "center",
														at: "center",
														of: window,
														collision: "fit"}
													);
												}
											}
										}
									}
								});
							});
							
							var sessionExpDialogBaseCfg = {
								modal: true,
								width: "auto",
								maxWidth: 750,
								position: {my: "center", at: "center", of: window, collision: "fit"},
								autoOpen: false,
								buttons: [
									{
										text: "Work Offline",
										title: "Keep this Window/Tab open for reference",
										click: function() {onSessionExpirationDialogClose(jQuery(this), "workOffline");}
								  },
								  {
										text: "Sign Back In",
										title: "Sign back into the DigitalEdge platform.  Tip: Middle-click to open in a new tab",
										click: function(event) {
											if(event && event.button && event.button === 1) {
												onSessionExpirationDialogClose(jQuery(this), "signInNewTab");
											}
											else {
												onSessionExpirationDialogClose(jQuery(this), "signIn");
											}
										}
								  }
								]
							};
							
							if(isWindowCloseable) {
								sessionExpDialogBaseCfg.buttons.push({
										text: "Close",
										title: "Close this window/tab",
										click: function() {onSessionExpirationDialogClose(jQuery(this), "close");}
							  	}
								);
							}
							else {
								sessionExpDialogBaseCfg.buttons.push({
										text: "Sign Out",
										title: "Proceed with Sign Out",
										click: function() {onSessionExpirationDialogClose(jQuery(this), "signOut");}
								  }
								);
							}
							
							var $sessionTimeoutDiv = $sessionExpTemplate.clone(true);
							$sessionTimeoutDiv.find("p:eq(0)").html("You have been automatically signed out due to inactivity.");
							$sessionTimeoutDialog = $sessionTimeoutDiv.dialog(
								jQuery.extend({title: "You've been Signed Out"}, sessionExpDialogBaseCfg));
							
							var $ssoCommsLostDiv = $sessionExpTemplate.clone(true);
							$ssoCommsLostDiv.find("p:eq(0)").html("Connectivity with the DigitalEdge servers has been lost.");
							$ssoCommsLostDialog = $ssoCommsLostDiv.dialog(
								jQuery.extend({title: "Server Connection Failed"}, sessionExpDialogBaseCfg));
							
							// Just using the template for the SSO Dialog
							$sessionExpTemplate.find("p:eq(0)").html("You've already Signed Out of DigitalEdge from another application.");
							$ssoLogoutDialog = $sessionExpTemplate.dialog(
									jQuery.extend({title: "You've been Signed Out"}, sessionExpDialogBaseCfg));
							
							
							// Configure the Session timer
							var sessionTimeoutMillis = Math.round((sessionSeconds * 1000) * .99);
							sessionTimer = new com.deleidos.rtws.jscommons.core.sessiontimeout.SessionTimer();
							sessionTimer.sessionDurationMillis = sessionTimeoutMillis;
							sessionTimer.sessionExpirationCallback = onSessionExpiration;
							
							// Setting the warning time to the lesser of 5 minutes or 15% of the session time
							var tmpWarnMillis = Math.min(300000, Math.round(sessionTimeoutMillis * .15));
							var warningDuration = sessionTimeoutMillis - tmpWarnMillis;
							if(warningDuration <= 10000) {
								// Turn off warnings if the session is too short to give the user enough time to respond.
								tmpWarnMillis = 0;
							}
							
							if(tmpWarnMillis > 0) {
								sessionTimer.warnMillis = tmpWarnMillis;
								sessionTimer.warnCallback = onSessionTimeoutWarning;
							}
							
							sessionTimer.ssoCheckMillis = 10000;
							sessionTimer.ssoCheckCallback = issueSsoCheck;
							sessionTimer.maxInconclusiveSsoChecks = 3;
							sessionTimer.nonResponsiveSsoChecksCallback = onSsoCommsLost;
							sessionTimer.ssoSessionExpirationCallback = onSsoLogout;
							
							sessionTimer.start();
						}
					}
					
					function issueSsoCheck() {
						jQuery.ajax("userData", {
									cache: false,
								 	context: this,
								 	timeout: 5000,
								 	success: function(data, textStatus, jqXHR) {
								 		var contentType = jqXHR.getResponseHeader("Content-Type");
								 		if(contentType && typeof contentType === "string") {
								 			if(contentType.toLowerCase().indexOf("application/json") >= 0) {
								 				sessionTimer.setSsoExpirationStatus(false);
								 			}
								 			else {
								 				// This request returned HTML ... Assuming redirection to the login page
								 				sessionTimer.setSsoExpirationStatus(true);
								 			}
								 		}
								 		else {
								 			/* Being lienient and not expiring the SSO session.  This case is unlikely
								 			and if the server doesn't return the content-type header, at least
								 			the only side affect will be no SSO detection.  Otherwise, it could break
								 			the app by forcing a logout.
								 			*/
								 			sessionTimer.setSsoExpirationStatus(false);
								 		}
								 	},
								 	error: function(data, textStatus, jqXHR) {
										if(textStatus === "error" && data && data.status === 0) {
											// assuming the error was a jquery error and cross domain redirect problem
											sessionTimer.setSsoExpirationStatus(true);
										}
										else {
											// Request timed out or failed ... setting an inconclusive SSO status
									 		sessionTimer.setSsoExpirationStatus(null);
										}
								 	},
								}
						);
						
					}
					
					function onSessionTimeoutWarning(millisRemaining)
					{
						if($timeoutWarningDialog != null)
						{
							$timeoutWarningDialog.dialog("open");
						}
					}
					
					function closeWarningDialog(restartSession) {
						if($timeoutWarningDialog && $timeoutWarningDialog != null && $timeoutWarningDialog.dialog("isOpen")) {
							$timeoutWarningDialog.dialog("close");
							
							if(typeof restartSession !== "undefined") {
								if(restartSession === true) {
									jQuery.ajax("userData", {cache: false, context: this, success: restartSessionTimer});
								}
								else if(restartSession === false) {
									signOut();
								}
							}
						}
					}
					
					function onSessionExpiration() {
						closeWarningDialog(); // Leaving arg undefined to take no further action
						if($sessionTimeoutDialog != null) {
							$sessionTimeoutDialog.dialog("open");
						}
					}
					
					function onSsoCommsLost() {
						closeWarningDialog(); // Leaving arg undefined to take no further action
						if($ssoCommsLostDialog != null) {
							$ssoCommsLostDialog.dialog("open");
						}
					}
					
					function onSsoLogout() {
						closeWarningDialog(); // Leaving arg undefined to take no further action
						if($ssoLogoutDialog != null) {
							$ssoLogoutDialog.dialog("open");
						}
					}
					
					function onSessionExpirationDialogClose($dialog, action) {
						if($dialog.dialog("isOpen")) {
							$dialog.dialog("close");
						}
						
						if(action === "workOffline") {
							// Nothing to do ... Leaving case for completion.
						}
						else if(action === "signIn") {
							if(window) {
								window.location.href = "<c:url value='/' />";
							}
						}
						else if(action === "signInNewTab") {
							if(window && window.open) {
								window.open("<c:url value='/' />", "_blank");
							}
						}
						else if(action === "signOut") {
							signOut();
						}
						else if(action === "close") {
							// NiceToHave: make a call to sign out
							if(window) {
								window.close();
							}
						}
					}
					
					function signOut()
					{
						window.location.href = "<c:url value="/j_spring_security_logout" />";
					}
					
					function restartSessionTimer()
					{
						if(sessionTimer != null)
						{
							sessionTimer.restart();
						}
					}
					
					return {
						initSessionTimer: initSessionTimer,
						restartSessionTimer: restartSessionTimer
					};
				}();
				
				jQuery(document).ready(function () {
					com.deleidos.rtws.jscommons.core.sessiontimeout.impl.initSessionTimer();
				});
			</script>
    </head>
