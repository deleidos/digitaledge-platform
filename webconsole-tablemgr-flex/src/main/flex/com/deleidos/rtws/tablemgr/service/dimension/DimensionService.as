package com.deleidos.rtws.tablemgr.service.dimension
{
	import com.deleidos.rtws.commons.event.OperationStatusEvent;
	import com.deleidos.rtws.commons.model.auth.IUser;
	import com.deleidos.rtws.commons.service.rest.SimpleRestService;
	import com.deleidos.rtws.commons.tenantutils.model.system.SystemState;
	import com.deleidos.rtws.commons.util.StringUtils;
	import com.deleidos.rtws.tablemgr.event.conn.ConnectionsStatusEvent;
	import com.deleidos.rtws.tablemgr.model.TableContext;
	import com.deleidos.rtws.tablemgr.model.conn.DbConnConfig;
	import com.deleidos.rtws.tablemgr.model.db.Table;
	import com.deleidos.rtws.tablemgr.model.map.DimensionTableMapping;
	import com.deleidos.rtws.tablemgr.service.ParseError;
	import com.deleidos.rtws.tablemgr.service.dimension.parse.DimTableDependenciesParser;
	import com.deleidos.rtws.tablemgr.service.dimension.parse.DimTableMappingInputBuilder;
	import com.deleidos.rtws.tablemgr.service.dimension.parse.DimTableMappingResultParser;
	import com.deleidos.rtws.tablemgr.service.dimension.parse.FetchConnectionsResultParser;
	
	import flash.events.Event;
	
	import mx.collections.ArrayList;
	import mx.rpc.events.FaultEvent;
	import mx.rpc.events.ResultEvent;
	
	public class DimensionService extends SimpleRestService implements IDimensionService
	{
		private static const SERVICE_BASE_PATH:String = "proxy/rtws/tenantapi/json";
		private static const SERVICE_DELETE_BASE_PATH:String = "proxy/rtws/tenantapi/rest";
		private static const DEFAULT_SERVICE_TIMEOUT:Number = 60;
		private static const TABLE_DEP_SYSTEM_STATE_EXCLUSIONS:ArrayList = new ArrayList([SystemState.DELETING, SystemState.DOWN]);
		
		[Inject]
		public var userModel:IUser;
		
		public function DimensionService(baseUrl:String=null, defaultTimeout:Number=NaN)
		{
			super(SERVICE_BASE_PATH, DEFAULT_SERVICE_TIMEOUT);
		}
		
		public function fetchConnections(tenantId:String=null, opTrackingToken:Object=null):void
		{
			var ctx:DimensionServiceOpContext = new DimensionServiceOpContext();
			ctx.opType = DimensionServiceOpContext.FETCH_CONNECTIONS_OP;
			ctx.opTrackingToken = opTrackingToken;
			
			var params:Object = {
				tenantId: (StringUtils.isNotBlank(tenantId) ? tenantId : userModel.tenantId)
			};
			this.get("/dimensions/connections", params, ctx);
		}
		
		public function fetchDimTableInfo(connConfig:DbConnConfig, table:Table, opTrackingToken:Object=null):void
		{
			var ctx:DimensionServiceOpContext = new DimensionServiceOpContext();
			ctx.opType = DimensionServiceOpContext.FETCH_DIM_TABLE_INFO_OP;
			ctx.opTrackingToken = opTrackingToken;
			ctx.connConfig = connConfig;
			ctx.table = table;
			
			var params:Object = {
				tableNames: table.getFullyQualifiedTableName()
			};
			this.get("/dimensions/connection/" + encodeURIComponent(connConfig.connId.toString()) + "/tables", params, ctx);
		}
		
		public function saveDimTableMapping(tableCtxt:TableContext, dimTableMapping:DimensionTableMapping, opTrackingToken:Object=null):void
		{
			var ctx:DimensionServiceOpContext = new DimensionServiceOpContext();
			ctx.opType = DimensionServiceOpContext.SAVE_DIM_TABLE_MAPPING_OP;
			ctx.opTrackingToken = opTrackingToken;
			ctx.connConfig = tableCtxt.connConfig;
			ctx.table = tableCtxt.table;
			
			var cacheConfigJson:String = DimTableMappingInputBuilder.buildDimTableMappingJson(dimTableMapping);
			if(StringUtils.isBlank(cacheConfigJson))
			{
				var invalidMappingEvent:DimensionServiceStatusEvent = new DimensionServiceStatusEvent(
					DimensionServiceStatusEvent.SAVE_DIM_TABLE_MAPPING, OperationStatusEvent.SUB_TYPE_FAILURE, opTrackingToken, "Invalid Dimension Mapping");
				(invalidMappingEvent as DimensionServiceStatusEvent).tableCtxt = tableCtxt;
				this.dispatch(invalidMappingEvent);
			}
			else
			{
				var params:Object = {
					"cacheConfigJson": cacheConfigJson
				};
				
				var url:String = "/dimensions/cacheConfig";
				url += "/" + encodeURIComponent(userModel.tenantId);
				url += "/" + encodeURIComponent(tableCtxt.connConfig.connId.toString());
				url += "/" + encodeURIComponent(tableCtxt.table.schema);
				url += "/" + encodeURIComponent(tableCtxt.table.name);
				this.post(url, params, ctx);
			}
		}
		
		public function fetchDimTableDependendencies(tenantId:String, table:Table, opTrackingToken:Object=null):void
		{
			if(StringUtils.isBlank(tenantId))
			{
				throw new ArgumentError("'tenantId' is required");
			}
			
			if(table == null || StringUtils.isBlank(table.name))
			{
				throw new ArgumentError("'table' is null or invalid");
			}
			
			var ctx:DimensionServiceOpContext = new DimensionServiceOpContext();
			ctx.opType = DimensionServiceOpContext.FETCH_DIM_TABLE_DEPENDENCIES_OP;
			ctx.opTrackingToken = opTrackingToken;
			ctx.table = table;
			
			var params:Object = null;
			if(table != null)
			{
				params = {dimTableFilter: table.name};
			}
			this.get("/systems/" + encodeURIComponent(tenantId) + "/dependencies/dimTables", params, ctx);
		}
		
		public function deleteDimTableMapping(tableCtxt:TableContext, opTrackingToken:Object=null):void
		{
			var ctx:DimensionServiceOpContext = new DimensionServiceOpContext();
			ctx.opType = DimensionServiceOpContext.DELETE_DIM_TABLE_MAPPING_OP;
			ctx.opTrackingToken = opTrackingToken;
			ctx.connConfig = tableCtxt.connConfig;
			ctx.table = tableCtxt.table;

			var url:String = "/dimensions/cacheConfig";
			url += "/" + encodeURIComponent(userModel.tenantId);
			url += "/" + encodeURIComponent(tableCtxt.connConfig.connId.toString());
			url += "/" + encodeURIComponent(tableCtxt.table.schema);
			url += "/" + encodeURIComponent(tableCtxt.table.name);

			this.remove(url, null,  ctx);
		}
		
		override protected function handleFailure(event:FaultEvent, context:Object):void
		{
			var serviceCtx:DimensionServiceOpContext = (context as DimensionServiceOpContext);
			if(serviceCtx == null || StringUtils.isBlank(serviceCtx.opType))
			{
				throw new ArgumentError("Invalid Service Context");
			}
			
			var failureMsg:String = null;
			
			if(event != null && event.fault != null)
			{
				failureMsg = event.fault.faultString;
			}
			
			var eventToDispatch:Event = null;
			switch(serviceCtx.opType)
			{
				case DimensionServiceOpContext.FETCH_CONNECTIONS_OP:
				{
					if(StringUtils.isBlank(failureMsg))
					{
						failureMsg = "List Connections Call Failed";
					}
					eventToDispatch = new ConnectionsStatusEvent(
						ConnectionsStatusEvent.FETCH_CONNECTIONS, OperationStatusEvent.SUB_TYPE_FAILURE,
						serviceCtx.opTrackingToken, failureMsg);
					break;
				}
				case DimensionServiceOpContext.FETCH_DIM_TABLE_INFO_OP:
				{
					if(StringUtils.isBlank(failureMsg))
					{
						failureMsg = "Could not list Dimension table details";
					}
					eventToDispatch = new DimensionServiceStatusEvent(
						DimensionServiceStatusEvent.FETCH_DIM_TABLE_MAPPING, OperationStatusEvent.SUB_TYPE_FAILURE, serviceCtx.opTrackingToken, failureMsg);
					(eventToDispatch as DimensionServiceStatusEvent).tableCtxt = new TableContext(serviceCtx.connConfig, serviceCtx.table);
					break;
				}
				case DimensionServiceOpContext.SAVE_DIM_TABLE_MAPPING_OP:
				{
					var serviceErrorMsgs:ArrayList = DimTableMappingResultParser.parseSaveDimTableMappingError(event);
					if(serviceErrorMsgs != null && serviceErrorMsgs.length > 0)
					{
						failureMsg = serviceErrorMsgs.source.join("\n");
					}
					else
					{
						failureMsg = "Could not save Dimension table details";
					}
					eventToDispatch = new DimensionServiceStatusEvent(
						DimensionServiceStatusEvent.SAVE_DIM_TABLE_MAPPING, OperationStatusEvent.SUB_TYPE_FAILURE, serviceCtx.opTrackingToken, failureMsg);
					(eventToDispatch as DimensionServiceStatusEvent).tableCtxt = new TableContext(serviceCtx.connConfig, serviceCtx.table);
					break;
				}
				case DimensionServiceOpContext.FETCH_DIM_TABLE_DEPENDENCIES_OP:
				{
					if(StringUtils.isBlank(failureMsg))
					{
						failureMsg = "Could not list Dimension table dependencies";
					}
					eventToDispatch = new DimTableDepServiceStatusEvent(
						DimTableDepServiceStatusEvent.FETCH_DIM_TABLE_DEPENDENCIES, OperationStatusEvent.SUB_TYPE_FAILURE, serviceCtx.opTrackingToken, failureMsg);
					(eventToDispatch as DimTableDepServiceStatusEvent).dimTable = serviceCtx.table;
					break;
				}
				case DimensionServiceOpContext.DELETE_DIM_TABLE_MAPPING_OP:
				{
					failureMsg = "Could not delete the Enrichment settings";
					eventToDispatch = new DimensionServiceStatusEvent(
						DimensionServiceStatusEvent.DELETE_DIM_TABLE_MAPPING, OperationStatusEvent.SUB_TYPE_FAILURE, serviceCtx.opTrackingToken, failureMsg);
					(eventToDispatch as DimensionServiceStatusEvent).tableCtxt = new TableContext(serviceCtx.connConfig, serviceCtx.table);
					break;
				}
				default:
				{
					break;
				}
			}
			
			if(eventToDispatch != null)
			{
				this.dispatch(eventToDispatch);
			}
		}
		
		override protected function handleSuccess(event:ResultEvent, context:Object):void
		{
			var serviceCtx:DimensionServiceOpContext = (context as DimensionServiceOpContext);
			if(serviceCtx == null || StringUtils.isBlank(serviceCtx.opType))
			{
				throw new ArgumentError("Invalid Service Context");
			}
						
			var eventToDispatch:Event = null;
			switch(serviceCtx.opType)
			{
				case DimensionServiceOpContext.FETCH_CONNECTIONS_OP:
				{
					try
					{
						var parsedConnections:ArrayList = FetchConnectionsResultParser.parse(event);
						eventToDispatch = new ConnectionsStatusEvent(
							ConnectionsStatusEvent.FETCH_CONNECTIONS,
							OperationStatusEvent.SUB_TYPE_SUCCESS, serviceCtx.opTrackingToken);
						(eventToDispatch as ConnectionsStatusEvent).connections = parsedConnections;
					}
					catch(parseError:ParseError)
					{
						eventToDispatch = new ConnectionsStatusEvent(
							ConnectionsStatusEvent.FETCH_CONNECTIONS, OperationStatusEvent.SUB_TYPE_FAILURE,
							serviceCtx.opTrackingToken, parseError.message);
					}
					
					break;
				}
				case DimensionServiceOpContext.FETCH_DIM_TABLE_INFO_OP:
				{
					var tableCtxt:TableContext = new TableContext(serviceCtx.connConfig, serviceCtx.table);
					try
					{
						var mapping:DimensionTableMapping = DimTableMappingResultParser.parse(event);
						if(mapping != null)
						{
							mapping.tableCtxt = tableCtxt;
						}
						
						var tmpSuccessEvent:DimensionServiceStatusEvent = new DimensionServiceStatusEvent(
							DimensionServiceStatusEvent.FETCH_DIM_TABLE_MAPPING, OperationStatusEvent.SUB_TYPE_SUCCESS, serviceCtx.opTrackingToken);
						tmpSuccessEvent.tableCtxt = tableCtxt;
						tmpSuccessEvent.dimTableMapping = mapping;
						
						eventToDispatch = tmpSuccessEvent;
					}
					catch(parseError:ParseError)
					{
						eventToDispatch = new DimensionServiceStatusEvent(
							DimensionServiceStatusEvent.FETCH_DIM_TABLE_MAPPING, OperationStatusEvent.SUB_TYPE_FAILURE,
								serviceCtx.opTrackingToken, parseError.message);
						(eventToDispatch as DimensionServiceStatusEvent).tableCtxt = tableCtxt;
					}
					
					break;
				}
				case DimensionServiceOpContext.SAVE_DIM_TABLE_MAPPING_OP:
				{
					eventToDispatch = new DimensionServiceStatusEvent(
						DimensionServiceStatusEvent.SAVE_DIM_TABLE_MAPPING, OperationStatusEvent.SUB_TYPE_SUCCESS, serviceCtx.opTrackingToken);
					(eventToDispatch as DimensionServiceStatusEvent).tableCtxt = new TableContext(serviceCtx.connConfig, serviceCtx.table);
					
					break;
				}
				case DimensionServiceOpContext.FETCH_DIM_TABLE_DEPENDENCIES_OP:
				{
					var parsedDependentSysDomains:ArrayList = null;
					try
					{
						parsedDependentSysDomains = DimTableDependenciesParser.parseDependentSystemDomains(event, TABLE_DEP_SYSTEM_STATE_EXCLUSIONS);
						
						eventToDispatch = new DimTableDepServiceStatusEvent(
							DimTableDepServiceStatusEvent.FETCH_DIM_TABLE_DEPENDENCIES, OperationStatusEvent.SUB_TYPE_SUCCESS, serviceCtx.opTrackingToken);
						(eventToDispatch as DimTableDepServiceStatusEvent).dimTable = serviceCtx.table;
						(eventToDispatch as DimTableDepServiceStatusEvent).dependentSystemDomains = parsedDependentSysDomains;
					}
					catch(error:Error)
					{
						var usrMsg:String = (error.message as String);
						if(StringUtils.isBlank(usrMsg))
						{
							usrMsg = "Failed to parse the dependency service response";
						}
						eventToDispatch = new DimTableDepServiceStatusEvent(
							DimTableDepServiceStatusEvent.FETCH_DIM_TABLE_DEPENDENCIES, OperationStatusEvent.SUB_TYPE_FAILURE, serviceCtx.opTrackingToken, usrMsg);
						(eventToDispatch as DimTableDepServiceStatusEvent).dimTable = serviceCtx.table;
					}
					break;
				}
				case DimensionServiceOpContext.DELETE_DIM_TABLE_MAPPING_OP:
				{
					eventToDispatch = new DimensionServiceStatusEvent(
						DimensionServiceStatusEvent.DELETE_DIM_TABLE_MAPPING, OperationStatusEvent.SUB_TYPE_SUCCESS, serviceCtx.opTrackingToken);
					(eventToDispatch as DimensionServiceStatusEvent).tableCtxt = new TableContext(serviceCtx.connConfig, serviceCtx.table);
					break;
				}
				default:
				{
					break;
				}
			}
			
			if(eventToDispatch != null)
			{
				this.dispatch(eventToDispatch);
			}
		}
	}
}