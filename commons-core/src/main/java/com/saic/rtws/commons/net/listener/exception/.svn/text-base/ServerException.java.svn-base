package com.saic.rtws.commons.net.listener.exception;

/**
 * This exception tells the client connection thread
 * that something horrible happened in the command listener
 * while servicing a command.
 */
public class ServerException extends Exception {

	private static final long serialVersionUID = 3607077956300775816L;

	public ServerException(String message) {
		super(message);
	}
	
	public ServerException(String message, Throwable cause) {
		super(message, cause);
	}
	
}
