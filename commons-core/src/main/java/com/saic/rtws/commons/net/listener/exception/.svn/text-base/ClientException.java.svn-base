package com.saic.rtws.commons.net.listener.exception;

/**
 * This exception is thrown by the client class notifying the
 * invoker that the command failed.
 */
public class ClientException extends Exception {

	private static final long serialVersionUID = -6464194505925960863L;

	public ClientException(String message) {
		super(message);
	}
	
	public ClientException(String message, Throwable cause) {
		super(message, cause);
	}
	
}
