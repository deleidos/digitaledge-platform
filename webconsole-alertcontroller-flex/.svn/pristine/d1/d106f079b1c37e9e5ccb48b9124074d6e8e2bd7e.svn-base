	import org.robotlegs.base.ContextEvent;
	import org.robotlegs.mvcs.Context;

	
	public class AlertControllerRootContext extends Context
	{
		public function AlertControllerRootContext(contextView:DisplayObjectContainer=null, autoStartup:Boolean=true)
		{
			super(contextView, autoStartup);
		}
		
		override public function startup():void
		{
			// models
			var propsModel:SystemAppPropertiesModel = new SystemAppPropertiesModel();
			injector.mapValue(IAppPropertiesModel, propsModel);
			injector.mapValue(SystemAppPropertiesModel, propsModel);
			
			var user:AlertSubscriber = new AlertSubscriber();
			injector.mapValue(IUser, user);
			injector.mapValue(AlertSubscriber, user);
			
			injector.mapSingleton(AlertCriteriaFilterModel);
			injector.mapSingleton(FilterEditModel);
			
			injector.mapSingleton(SubscribersModel);
			
			injector.mapSingleton(DataModelsModel);
			
			// Old Models
//			injector.mapSingleton(ParameterModel);
//			injector.mapSingleton(ParameterEmailModel);
//			
//			injector.mapSingleton(WatchListEmailModel);
//			injector.mapSingleton(HTTPQueueModel);
//			injector.mapSingleton(CurrentDataModel);
			
			// services
			injector.mapClass(IPropertiesService, PropertiesService);
			injector.mapClass(IUserServices, UserServices);
			injector.mapClass(IThemeService, ThemeService);
			
			injector.mapClass(ISubscriberService, SubscriberService);
			injector.mapClass(ISubscribersService, SubscribersService);
			injector.mapClass(ISubscriptionsService, SubscriptionsService);
			
			injector.mapClass(IRepositoryService, RepositoryService);

			injector.mapSingletonOf(IDataModelServices, DataModelRestService);
			injector.mapSingletonOf(IFiltersService, FiltersService);
			injector.mapSingletonOf(IFilterService, FilterService);
			
			injector.mapClass(IJsonUtilsService, JsonUtilsService);

			// command-event
			commandMap.mapEvent(PropertiesEvent.FETCH_REMOTE_PROPS_REQUEST, LoadRemotePropsIntoModelCommand);
			commandMap.mapEvent(UserOpRequestEvent.LOAD_USER_INFO, UserOpRequestCommand);
			commandMap.mapEvent(ThemeOpRequestEvent.LOAD_THEME, ThemeOpRequestCommand);
			commandMap.mapEvent(ClientSessionTimerEvent.RESTART_SESSION_TIMER, RestartClientSessionTimerCommand);
			
			commandMap.mapEvent(LoadGlobalModelEvent.SETUP_ALERTING_USER, SetupAlertingUserCommand);
			commandMap.mapEvent(LoadGlobalModelEvent.LOAD_SUBSCRIBERS, LoadGlobalSubscribersCommand);
			commandMap.mapEvent(LoadGlobalModelEvent.LOAD_DATA_MODELS, LoadGlobalDataModelsCommand);
			
			commandMap.mapEvent(ManageViewsEvent.CONFIGURE_USER_EMAILS, UserSettingsCommand);
			commandMap.mapEvent(SubscriberEvent.UPDATE_SUBSCRIBER, UpdateAlertSubscriberCommand);
			
			commandMap.mapEvent(FiltersEvent.LOAD_FILTERS, FiltersCommand, FiltersEvent);
			
			commandMap.mapEvent(FilterDataRequestEvent.LOAD_SUBSCRIBERS, FilterDataFetchCommand);
			
			commandMap.mapEvent(FilterPropertyChangeEvent.FIELD_CHANGE, FilterPropEditCommand);
			
			commandMap.mapEvent(FilterEditEvent.SAVE, FilterEditCommand);
			commandMap.mapEvent(FilterSubscriptionsPersistenceEvent.MERGE_EDITS, MergedFilterSubsPersistenceCommand);
			
			commandMap.mapEvent(FilterEditEvent.CANCEL, FilterEditCommand);
			
			commandMap.mapEvent(FilterPersistenceEvent.CREATE_FILTER, FilterPersistenceCommand);
			commandMap.mapEvent(FilterPersistenceEvent.UPDATE_FILTER, FilterPersistenceCommand);
			commandMap.mapEvent(FilterPersistenceEvent.REMOVE_FILTER, FilterPersistenceCommand);
			
			commandMap.mapEvent(FilterPersistenceEvent.FILTER_REMOVED, FilterModelCommand);
			
			commandMap.mapEvent(JsonEvent.VALIDATE_JSON, JsonUtilsCommand);
			commandMap.mapEvent(JsonEvent.FORMAT_JSON, JsonUtilsCommand);
			
			// Views
			
			mediatorMap.mapView(AppLaunchStatusDisplay, AppLaunchStatusDisplayMediator);
			
			mediatorMap.mapView(AlertController, AlertControllerMediator);
			mediatorMap.mapView(FilterEditorsView, FilterEditorsViewMediator);
			
			mediatorMap.mapView(UserDisplay, UserDisplayMediator, IUserDisplay);
			mediatorMap.mapView(AboutDialog, AboutDialogMediator);
			mediatorMap.mapView(UserEmailsView, UserEmailsViewMediator);
			
			
			mediatorMap.mapView(FilterDeleteConfirmationView, FilterDeleteConfirmationViewMediator);
			mediatorMap.mapView(FilterListView, FilterListViewMediator);
			mediatorMap.mapView(FilterOverView, FilterOverViewMediator);
			mediatorMap.mapView(FilterNameModelView, FilterNameModelViewMediator);
			mediatorMap.mapView(FilterCriteriaView, FilterCriteriaViewMediator);
			mediatorMap.mapView(FilterDefinitionHelpView, FilterDefinitionHelpViewMediator);
			mediatorMap.mapView(FilterEmailView, FilterEmailViewMediator);
			mediatorMap.mapView(FilterEmailRecipientView, FilterEmailRecipientViewMediator);
			
			mediatorMap.mapView(FilterSaveConfirmationDialog, FilterSaveConfirmationDialogMediator);
			
			
			// Old Views
//			mediatorMap.mapView(CreateWatchListEntryPopUpView, CreateWatchListEntryPopUpViewMediator, null, false, false);
//			mediatorMap.mapView(DeleteAlertCriteriaPopUpView, DeleteAlertCriteriaPopUpViewMediator, null, false, false);
//			mediatorMap.mapView(NotificationView, NotificationViewMediator);
//			mediatorMap.mapView(RefreshAlertCriteriaPopUpView, RefreshAlertCriteriaPopUpViewMediator, null, false, false);
//			mediatorMap.mapView(UpdateAlertCriteriaPopUpView, UpdateAlertCriteriaPopUpViewMediator, null, false, false);
//			mediatorMap.mapView(BrowseDataModelPopUpView, BrowseDataModelPopUpViewMediator, null, false, false);
//			mediatorMap.mapView(CreateAlertDefinitionHelpPopUpView, CreateAlertDefinitionHelpPopUpViewMediator, null, false, false);
//			mediatorMap.mapView(CreateAlertValueSuggestionPopUpView, CreateAlertValueSuggestionPopUpViewMediator, null, false, false);
//			mediatorMap.mapView(FormatHelpPopUpView, FormatHelpPopUpViewMediator, null, false, false);
//			mediatorMap.mapView(CreateAlertDefinitionView, CreateAlertDefinitionViewMediator);
// mediatorMap.mapView(CreateAlertCriteriaView, CreateAlertCriteriaViewMediator);
// mediatorMap.mapView(CreateAlertDefinitionPopUpView, CreateAlertDefinitionPopUpViewMediator);

//			mediatorMap.mapView(FiltersView, FiltersViewMediator);

			mediatorMap.createMediator(contextView as AlertController);

			commandMap.mapEvent(ContextEvent.STARTUP_COMPLETE, StartupCommand, ContextEvent, true);

			super.startup();
		}
	}
}
