package com.saic.rtws.commons.model.tmsdb;

import java.sql.Timestamp;

public class Registration {
	
	private String emailAddress;
	private String uuid;
	private Timestamp expirationTimestamp;
	private Timestamp emailTimestamp;
	private Integer accountId;
	
	public void setEmailAddress(String emailAddress) {
		this.emailAddress = emailAddress;
	}
	
	public String getEmailAddress() {
		return this.emailAddress;
	}
	
	public void setUUID(String uuid) {
		this.uuid = uuid;
	}
	
	public String getUUID() {
		return this.uuid;
	}
	
	public void setExpirationTimestamp(Timestamp expirationTimestamp) {
		this.expirationTimestamp = expirationTimestamp;
	}
	
	public Timestamp getExpirationTimestamp() {
		return this.expirationTimestamp;
	}
	
	public void setEmailTimestamp(Timestamp emailTimestamp) {
		this.emailTimestamp = emailTimestamp;
	}
	
	public Timestamp getEmailTimestamp() {
		return this.emailTimestamp;
	}

	public Integer getAccountId() {
		return accountId;
	}

	public void setAccountId(Integer accountId) {
		this.accountId = accountId;
	}
	
}