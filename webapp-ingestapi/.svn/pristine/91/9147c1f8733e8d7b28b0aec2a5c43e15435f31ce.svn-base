	@Test
	public void testClassName() {
		String [] datasinks = {"com.deleidos.rtws.ext.datasink.ExternalHdfsDataSink", "com.deleidos.rtws.ext.datasink.MongoDbDataSink", "com.acme.MyFreshDataSink"};
		for (String dsink : datasinks) {
			String[] parts = dsink.split("\\.");
			String dsinkName = parts[parts.length - 1];
			assertNotNull(dsinkName);
			System.out.println(dsinkName);
		}
	}
	
