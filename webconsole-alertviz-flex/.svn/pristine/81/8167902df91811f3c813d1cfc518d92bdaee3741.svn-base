package com.deleidos.rtws.alertviz.models
{
	/**
	 * This class represents one alert definition that goes into the DataGrid
	 */
	public class AlertDefinition
	{
		public var field:String;
		public var operator:String;
		public var value:String;
		
		/**
		 * Boolean stating whether this definition is a new definition or whether it was edited
		 */
		public var wasEdited:Boolean = false;
		
		/**
		 * The data type of this definition. Either string, number, or datestring
		 */
		public var type:String;
		
		/**
		 * The data model this definition uses
		 */
		public var model:String;
	}
}
