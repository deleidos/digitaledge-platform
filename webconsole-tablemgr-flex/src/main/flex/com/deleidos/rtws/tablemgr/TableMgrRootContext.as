	public class TableMgrRootContext extends Context
	{
		public function TableMgrRootContext(contextView:DisplayObjectContainer=null, autoStartup:Boolean=true)
		{
			super(contextView, autoStartup);
		}
		
		override public function startup():void
		{
			// Models
			injector.mapSingletonOf(IAppPropertiesModel, TmsAppPropertiesModel);
			injector.mapSingletonOf(IUser, User);
			injector.mapSingletonOf(DatabaseModels, DatabaseModels);
			injector.mapClass(DatabaseModel, DatabaseModel);
			injector.mapClass(TableModel, TableModel);
			
			// Services
			injector.mapClass(IPropertiesService, PropertiesService);
			//injector.mapSingletonOf(IUserServices, MockUserServices);
			injector.mapSingletonOf(IUserServices, UserServices);
			
			injector.mapClass(IThemeService, ThemeService);
			
			injector.mapClass(ISequenceService, SequenceService);
			
			//injector.mapClass(ITableService, MockTableService);
			injector.mapClass(ITableService, TableService);
			injector.mapClass(ITableCrudService, TableCrudService);
			
			injector.mapClass(ITableIndexService, TableIndexService);
			
			injector.mapClass(IDimensionService, DimensionService);
			
			// Commands
			commandMap.mapEvent(PropertiesEvent.FETCH_REMOTE_PROPS_REQUEST, LoadRemotePropsIntoModelCommand);
			commandMap.mapEvent(PopupEvent.CREATE_MEDIATOR, PopupCommand, PopupEvent);
			commandMap.mapEvent(PopupEvent.DISPOSE_MEDIATOR, PopupCommand, PopupEvent);
			
			commandMap.mapEvent(ClientSessionTimerEvent.RESTART_SESSION_TIMER, RestartClientSessionTimerCommand);
			
			commandMap.mapEvent(UserOpRequestEvent.LOAD_USER_INFO, UserOpRequestCommand);
			commandMap.mapEvent(ThemeOpRequestEvent.LOAD_THEME, ThemeOpRequestCommand);
			
			commandMap.mapEvent(ConnectionEvent.FETCH_CONNECTIONS, FetchConnectionsCommand, ConnectionEvent);
			commandMap.mapEvent(ConnectionEvent.CONNECT, ConnectCommand);
			commandMap.mapEvent(ConnectionEvent.DISCONNECT, DisconnectCommand);
			commandMap.mapEvent(ConnSelectionEvent.SELECTION_CHANGE, SelectCommand);
			
			commandMap.mapEvent(SequenceStatusEvent.FETCH_SEQUENCES, FetchSequencesCommand);
			commandMap.mapEvent(SequenceStatusEvent.CREATE_SEQUENCE, CreateSequenceCommand);
			
			commandMap.mapEvent(LoadTableDataEvent.LIST_TABLES, ListTablesCommand);
			
			commandMap.mapEvent(LoadTableDataEvent.LIST_TABLE_COLUMNS, LoadTableDataCommand, LoadTableDataEvent);
			commandMap.mapEvent(LoadTableDataEvent.LIST_TABLE_INDEXES, LoadTableDataCommand, LoadTableDataEvent);
			commandMap.mapEvent(LoadTableDataEvent.LIST_TABLE_CONSTRAINTS, LoadTableDataCommand, LoadTableDataEvent);
			commandMap.mapEvent(LoadTableDataEvent.LOAD_FULL_TABLE_DETAILS, LoadTableDataCommand, LoadTableDataEvent);
			
			commandMap.mapEvent(DimTableMappingEvent.LOAD_REQUEST, LoadDimTableMappingInfoCommand, DimTableMappingEvent);
			commandMap.mapEvent(DimTableMappingEvent.SAVE_REQUEST, SaveDimTableMappingCommand, DimTableMappingEvent);
			commandMap.mapEvent(DimTableMappingEvent.DELETE_REQUEST, DeleteDimTableMappingCommand, DimTableMappingEvent);
			
			commandMap.mapEvent(TableEvent.SAVE_TABLE, SaveTableCommand, Event);
			commandMap.mapEvent(TableDataStatusEvent.SAVE_TABLE, SaveTableCommand, Event);
			
			commandMap.mapEvent(TableEvent.DELETE_TABLE, DeleteTableCommand, Event);
			commandMap.mapEvent(TableDataStatusEvent.DELETE_TABLE, DeleteTableCommand, Event);
			
			commandMap.mapEvent(ColumnEvent.ADD_COLUMN, UpdateTableColumnCommand, Event);
			commandMap.mapEvent(ColumnUpdateStatusEvent.ADD_COLUMN, UpdateTableColumnCommand, Event);
			commandMap.mapEvent(ColumnEvent.UPDATE_COLUMN, UpdateTableColumnCommand, Event);
			commandMap.mapEvent(ColumnUpdateStatusEvent.UPDATE_COLUMN, UpdateTableColumnCommand, Event);
			commandMap.mapEvent(ColumnEvent.DELETE_COLUMN, UpdateTableColumnCommand, Event);
			commandMap.mapEvent(ColumnUpdateStatusEvent.DELETE_COLUMN, UpdateTableColumnCommand, Event);
			
			commandMap.mapEvent(TableIndexEvent.CREATE_INDEX, CreateTableIndexCommand, TableIndexEvent);
			//commandMap.mapEvent(TableIndexEvent.UPDATE_INDEX, UpdateTableIndexCommand, TableIndexEvent);
			commandMap.mapEvent(TableIndexEvent.DELETE_INDEX, DeleteTableIndexCommand, TableIndexEvent);
			
			commandMap.mapEvent(TableBatchDataEvent.TRUNCATE, TableBatchDataCommand);
			commandMap.mapEvent(TableBatchDataEvent.REQUEST_IMPORT_DATA, TableBatchDataCommand);
			
			// Mediators
			mediatorMap.mapView(AboutDialog, AboutDialogMediator, null, false, false);
			mediatorMap.mapView(UserDisplay, UserDisplayMediator, IUserDisplay);
			
			mediatorMap.mapView(CreateConnectionWelcomeView, CreateConnectionViewMediator, ICreateConnectionView);
			mediatorMap.mapView(CreateConnectionDialog, CreateConnectionViewMediator, ICreateConnectionView);
			
			mediatorMap.mapView(ConnChooser, ConnChooserMediator, IConnectionsView);
			
			mediatorMap.mapView(ConnectionsWorkspace, ConnectionsMediator, IConnectionsView);
			mediatorMap.mapView(DbOverview, DbOverviewMediator);
			mediatorMap.mapView(TablesView, TablesViewMediator);
			mediatorMap.mapView(TableEditor, TableEditorMediator);
			
			mediatorMap.mapView(ConfirmTableDeleteDialog, ConfirmTableDeleteDialogMediator);
			
			mediatorMap.mapView(ColumnEditor, ColumnEditorMediator);
			mediatorMap.mapView(EditColumnDialog, EditColumnDialogMediator, null, false, false);
			mediatorMap.mapView(SequenceSelector, SequenceSelectorMediator);
			mediatorMap.mapView(ColumnEditConfirmDialog, ColumnEditConfirmDialogMediator);
			
			mediatorMap.mapView(IndexesList, IndexesListMediator);
			mediatorMap.mapView(IndexEditor, IndexEditorMediator);
			
			mediatorMap.mapView(ConfirmIndexModDialog, ConfirmIndexModDialogMediator);
			
			mediatorMap.mapView(MappingEditor, MappingEditorMediator);
			mediatorMap.mapView(TableBatchDataView, TableBatchDataMediator);
			
			// Root View Mediator Declarations and Instantiation
			mediatorMap.mapView(AppLaunchStatusDisplay, AppLaunchStatusDisplayMediator);
			mediatorMap.mapView(TableManager, TableMgrMediator);
			mediatorMap.createMediator(contextView);
			mediatorMap.createMediator((contextView as TableManager).launchStatusDisplay);
			
			commandMap.mapEvent(ContextEvent.STARTUP_COMPLETE, StartupCommand, ContextEvent, true);
			
			super.startup();
		}
	}
