<%
	// Add Requested properties and their values to the map
	Map<String, String> flashVarsMap = FlashVarsUtil.buildPropertiesKeyValueMap("@html-wrapper.flashvars.propertykeys@");
	
	// Add Parent server props to the map for legacy support
	flashVarsMap.put("requestScheme", request.getScheme());
	flashVarsMap.put("requestServerName", request.getServerName());
	flashVarsMap.put("requestServerPort", Integer.toString(request.getServerPort()));
	
	// Add Requested querystring params to the map
	Map<String, String> requestedQueryStringParamMap = FlashVarsUtil.buildMap("@html-wrapper.flashvars.requestParams@");
	for(String currKey : requestedQueryStringParamMap.keySet())
	{
		String currValue = request.getParameter(currKey);
		
		if(currValue != null)
		{
			flashVarsMap.put(currKey, currValue);
		}
	}
	
	String flexAjaxBridgeName = "@html-wrapper.flexAjaxBridge.bridgeName@";
	if(StringUtils.isBlank(flexAjaxBridgeName))
	{
		flexAjaxBridgeName = null;
	}
	else
	{
		flashVarsMap.put("bridgeName",  flexAjaxBridgeName);
	}
	pageContext.setAttribute("flexAjaxBridgeName", flexAjaxBridgeName);
	
	System.out.println(flashVarsMap);
	
	String setFlashVarsJavascript = FlashVarsUtil.buildSetFlashVarsJavascript(flashVarsMap, "flashvars");
	String flashVarsQueryString = FlashVarsUtil.buildFlashVarsQueryString(flashVarsMap);
	
	List<String> endBodyJsSrcPaths = new ArrayList<String>();
	
	String endBodyJsCfg = "@html-wrapper.jsIncludes.endBody.srcPaths@";
	if(StringUtils.isNotBlank(endBodyJsCfg))
	{
		String[] jsTokens = endBodyJsCfg.split(",");
		
		if(jsTokens != null && jsTokens.length > 0) {
			for(int index=0; index < jsTokens.length; index++) {
				if(StringUtils.isNotBlank(jsTokens[index])) {
					endBodyJsSrcPaths.add(jsTokens[index].trim());
				}
			}
		}
	}
	
	pageContext.setAttribute("endBodyJsSrcPaths", endBodyJsSrcPaths);
%>

